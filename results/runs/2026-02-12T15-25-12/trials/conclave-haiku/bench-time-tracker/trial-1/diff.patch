+import { readFileSync, writeFileSync, mkdirSync, existsSync } from 'fs';
+import { dirname } from 'path';
+
+export interface Entry {
+  taskName: string;
+  startTime: string;
+  endTime: string | null;
+}
+
+export class TimeTracker {
+  private filePath: string;
+  private entries: Entry[] = [];
+  private activeTaskIndex: number | null = null;
+
+  constructor(filePath: string) {
+    this.filePath = filePath;
+    this.loadEntries();
+  }
+
+  private loadEntries(): void {
+    if (existsSync(this.filePath)) {
+      try {
+        const data = readFileSync(this.filePath, 'utf-8');
+        this.entries = JSON.parse(data);
+        // Find the active task (entry without endTime)
+        this.activeTaskIndex = this.entries.findIndex((e) => e.endTime === null);
+        if (this.activeTaskIndex === -1) {
+          this.activeTaskIndex = null;
+        }
+      } catch {
+        this.entries = [];
+        this.activeTaskIndex = null;
+      }
+    }
+  }
+
+  private saveEntries(): void {
+    const dir = dirname(this.filePath);
+    if (!existsSync(dir)) {
+      mkdirSync(dir, { recursive: true });
+    }
+    writeFileSync(this.filePath, JSON.stringify(this.entries, null, 2));
+  }
+
+  start(taskName: string): Entry {
+    if (taskName === '') {
+      throw new Error('Task name cannot be empty');
+    }
+
+    // Auto-stop the currently running task if any
+    if (this.activeTaskIndex !== null) {
+      this.entries[this.activeTaskIndex].endTime = new Date().toISOString();
+    }
+
+    // Create new entry
+    const entry: Entry = {
+      taskName,
+      startTime: new Date().toISOString(),
+      endTime: null,
+    };
+
+    this.entries.push(entry);
+    this.activeTaskIndex = this.entries.length - 1;
+    this.saveEntries();
+
+    return entry;
+  }
+
+  stop(): Entry {
+    if (this.activeTaskIndex === null) {
+      throw new Error('No task is currently running');
+    }
+
+    const entry = this.entries[this.activeTaskIndex];
+    entry.endTime = new Date().toISOString();
+    this.activeTaskIndex = null;
+    this.saveEntries();
+
+    return entry;
+  }
+
+  getActiveTask(): Entry | null {
+    if (this.activeTaskIndex === null) {
+      return null;
+    }
+    return this.entries[this.activeTaskIndex];
+  }
+
+  getLog(options?: { since?: Date }): Entry[] {
+    if (!options || !options.since) {
+      return this.entries;
+    }
+
+    const sinceTime = options.since.getTime();
+
+    return this.entries.filter((entry) => {
+      const startTime = new Date(entry.startTime).getTime();
+
+      // Include if started on or after the since date
+      if (startTime >= sinceTime) {
+        return true;
+      }
+
+      // Include if still running (endTime is null)
+      if (entry.endTime === null) {
+        return true;
+      }
+
+      // Include if endTime is on or after the since date
+      const endTime = new Date(entry.endTime).getTime();
+      if (endTime >= sinceTime) {
+        return true;
+      }
+
+      return false;
+    });
+  }
+
+  getSummary(): Record<string, number> {
+    const summary: Record<string, number> = {};
+
+    for (const entry of this.entries) {
+      const startTime = new Date(entry.startTime).getTime();
+      const endTime = entry.endTime
+        ? new Date(entry.endTime).getTime()
+        : Date.now();
+
+      const duration = endTime - startTime;
+
+      if (summary[entry.taskName]) {
+        summary[entry.taskName] += duration;
+      } else {
+        summary[entry.taskName] = duration;
+      }
+    }
+
+    return summary;
+  }
+}
