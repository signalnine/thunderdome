+import { randomUUID } from 'node:crypto';
+import { parseCron } from './cron.js';
+import type {
+  Task,
+  TaskOptions,
+  MiddlewareFn,
+  HandlerFn,
+  ProgressListener,
+  Schedule,
+  ShutdownOptions,
+  ShutdownResult,
+} from './types.js';
+
+interface QueueState {
+  concurrency?: number;
+  middlewares: MiddlewareFn[];
+  handler?: HandlerFn;
+  dlq: string[];
+}
+
+export class TaskQueue {
+  private tasks = new Map<string, Task>();
+  private queueStates = new Map<string, QueueState>();
+  private progressListeners = new Map<string, Map<number, ProgressListener>>();
+  private nextListenerId = 0;
+  private schedules = new Map<string, Schedule>();
+  private shuttingDown = false;
+  private shutdownPromise: Promise<ShutdownResult> | null = null;
+  private shutdownCallbacks: (() => void | Promise<void>)[] = [];
+
+  private getOrCreateQueue(name: string): QueueState {
+    let q = this.queueStates.get(name);
+    if (!q) {
+      q = { middlewares: [], dlq: [] };
+      this.queueStates.set(name, q);
+    }
+    return q;
+  }
+
+  // â”€â”€ Phase 1 & 2: Add task â”€â”€
+
+  add(payload: unknown, options: TaskOptions = {}): Task {
+    if (this.shuttingDown) {
+      throw new Error('Queue is shutting down');
+    }
+
+    const queueName = options.queue ?? 'default';
+    const priority = options.priority ?? 5;
+
+    if (!Number.isInteger(priority) || priority < 1 || priority > 10) {
+      throw new Error(
+        `Invalid priority: ${priority}. Must be an integer between 1 and 10.`,
+      );
+    }
+
+    const now = new Date();
+    let availableAt = now;
+    if (options.availableAt) {
+      availableAt = options.availableAt;
+    } else if (options.delay != null && options.delay > 0) {
+      availableAt = new Date(now.getTime() + options.delay);
+    }
+
+    // Phase 7: Validate dependencies
+    const dependencies = options.dependencies ?? [];
+    for (const depId of dependencies) {
+      if (!this.tasks.has(depId)) {
+        throw new Error(`Dependency task not found: ${depId}`);
+      }
+    }
+
+    const task: Task = {
+      id: randomUUID(),
+      payload,
+      status: 'pending',
+      priority,
+      queue: queueName,
+      createdAt: now,
+      updatedAt: now,
+      availableAt,
+      attempts: 0,
+      maxRetries: options.maxRetries ?? 3,
+      backoffDelay: options.backoffDelay ?? 1000,
+      dependencies: [...dependencies],
+      progress: 0,
+    };
+
+    // Check for circular dependencies
+    if (dependencies.length > 0) {
+      this.tasks.set(task.id, task);
+      if (this.hasCircularDependency(task.id)) {
+        this.tasks.delete(task.id);
+        throw new Error('Circular dependency detected');
+      }
+    } else {
+      this.tasks.set(task.id, task);
+    }
+
+    this.getOrCreateQueue(queueName);
+
+    return task;
+  }
+
+  // â”€â”€ Phase 7: Circular dependency detection â”€â”€
+
+  private hasCircularDependency(startId: string): boolean {
+    const visited = new Set<string>();
+    const stack = [...(this.tasks.get(startId)?.dependencies ?? [])];
+
+    while (stack.length > 0) {
+      const current = stack.pop()!;
+      if (current === startId) return true;
+      if (visited.has(current)) continue;
+      visited.add(current);
+
+      const task = this.tasks.get(current);
+      if (task) {
+        for (const depId of task.dependencies) {
+          stack.push(depId);
+        }
+      }
+    }
+
+    return false;
+  }
+
+  // â”€â”€ Phase 1 & 2: Retrieve next task â”€â”€
+
+  next(queueName?: string): Task | null {
+    const queue = queueName ?? 'default';
+    const queueState = this.queueStates.get(queue);
+
+    // Phase 8: Check concurrency limit
+    if (queueState?.concurrency != null) {
+      const active = this.getActiveCount(queue);
+      if (active >= queueState.concurrency) return null;
+    }
+
+    const now = new Date();
+    const candidates: Task[] = [];
+
+    for (const task of this.tasks.values()) {
+      if (task.queue !== queue) continue;
+      if (task.status !== 'pending') continue;
+      if (task.availableAt > now) continue;
+      if (!this.areDependenciesMet(task)) continue;
+      candidates.push(task);
+    }
+
+    if (candidates.length === 0) return null;
+
+    // Sort by priority (asc), then availableAt (asc), then createdAt (asc)
+    candidates.sort((a, b) => {
+      if (a.priority !== b.priority) return a.priority - b.priority;
+      const availDiff = a.availableAt.getTime() - b.availableAt.getTime();
+      if (availDiff !== 0) return availDiff;
+      return a.createdAt.getTime() - b.createdAt.getTime();
+    });
+
+    const task = candidates[0];
+    task.status = 'processing';
+    task.updatedAt = new Date();
+    task.attempts++;
+
+    return task;
+  }
+
+  private areDependenciesMet(task: Task): boolean {
+    return task.dependencies.every((depId) => {
+      const dep = this.tasks.get(depId);
+      return dep?.status === 'completed';
+    });
+  }
+
+  private getActiveCount(queueName: string): number {
+    let count = 0;
+    for (const task of this.tasks.values()) {
+      if (task.queue === queueName && task.status === 'processing') {
+        count++;
+      }
+    }
+    return count;
+  }
+
+  // â”€â”€ Phase 1: Queue size â”€â”€
+
+  size(queueName?: string): number {
+    const queue = queueName ?? 'default';
+    let count = 0;
+    for (const task of this.tasks.values()) {
+      if (task.queue === queue && task.status === 'pending') {
+        count++;
+      }
+    }
+    return count;
+  }
+
+  // â”€â”€ Phase 2: List queues â”€â”€
+
+  listQueues(): string[] {
+    return [...this.queueStates.keys()];
+  }
+
+  // â”€â”€ Task lookup â”€â”€
+
+  getTask(taskId: string): Task | null {
+    return this.tasks.get(taskId) ?? null;
+  }
+
+  // â”€â”€ Phase 5: Complete a task â”€â”€
+
+  complete(taskId: string): void {
+    const task = this.tasks.get(taskId);
+    if (!task) throw new Error(`Task ${taskId} not found`);
+    if (task.status !== 'processing') {
+      throw new Error(`Task ${taskId} is not in processing state`);
+    }
+
+    task.status = 'completed';
+    task.updatedAt = new Date();
+    task.progress = 100;
+  }
+
+  // â”€â”€ Phase 5: Fail a task (with retry/backoff) â”€â”€
+
+  fail(taskId: string, error?: string): void {
+    const task = this.tasks.get(taskId);
+    if (!task) throw new Error(`Task ${taskId} not found`);
+    if (task.status !== 'processing') {
+      throw new Error(`Task ${taskId} is not in processing state`);
+    }
+
+    task.error = error;
+    task.updatedAt = new Date();
+
+    if (task.attempts <= task.maxRetries) {
+      // Retry with exponential backoff
+      const delay = task.backoffDelay * Math.pow(2, task.attempts - 1);
+      task.status = 'pending';
+      task.availableAt = new Date(Date.now() + delay);
+      task.progress = 0;
+      task.progressMessage = undefined;
+    } else {
+      // Exhausted retries â€” move to DLQ
+      task.status = 'failed';
+      const queueState = this.getOrCreateQueue(task.queue);
+      queueState.dlq.push(task.id);
+    }
+  }
+
+  // â”€â”€ Phase 6: Dead letter queue â”€â”€
+
+  getDeadLetterTasks(queueName?: string): Task[] {
+    const queue = queueName ?? 'default';
+    const state = this.queueStates.get(queue);
+    if (!state) return [];
+    return state.dlq
+      .map((id) => this.tasks.get(id))
+      .filter((t): t is Task => t != null);
+  }
+
+  requeue(taskId: string): Task {
+    for (const state of this.queueStates.values()) {
+      const idx = state.dlq.indexOf(taskId);
+      if (idx !== -1) {
+        state.dlq.splice(idx, 1);
+        const task = this.tasks.get(taskId)!;
+        task.status = 'pending';
+        task.attempts = 0;
+        task.error = undefined;
+        task.availableAt = new Date();
+        task.updatedAt = new Date();
+        task.progress = 0;
+        task.progressMessage = undefined;
+        return task;
+      }
+    }
+    throw new Error(`Task ${taskId} not found in any dead letter queue`);
+  }
+
+  clearDeadLetterQueue(queueName?: string): void {
+    const queue = queueName ?? 'default';
+    const state = this.queueStates.get(queue);
+    if (state) {
+      state.dlq = [];
+    }
+  }
+
+  // â”€â”€ Phase 7: Dependency status â”€â”€
+
+  getDependencyStatus(
+    taskId: string,
+  ): { met: string[]; unmet: string[] } | null {
+    const task = this.tasks.get(taskId);
+    if (!task) return null;
+
+    const met: string[] = [];
+    const unmet: string[] = [];
+
+    for (const depId of task.dependencies) {
+      const dep = this.tasks.get(depId);
+      if (dep?.status === 'completed') {
+        met.push(depId);
+      } else {
+        unmet.push(depId);
+      }
+    }
+
+    return { met, unmet };
+  }
+
+  // â”€â”€ Phase 8: Concurrency control â”€â”€
+
+  setConcurrency(queueName: string, limit: number): void {
+    const state = this.getOrCreateQueue(queueName);
+    state.concurrency = limit;
+  }
+
+  getConcurrency(queueName: string): { limit: number | null; active: number } {
+    const state = this.queueStates.get(queueName);
+    return {
+      limit: state?.concurrency ?? null,
+      active: this.getActiveCount(queueName),
+    };
+  }
+
+  // â”€â”€ Phase 9: Progress tracking â”€â”€
+
+  updateProgress(taskId: string, progress: number, message?: string): void {
+    const task = this.tasks.get(taskId);
+    if (!task) throw new Error(`Task ${taskId} not found`);
+    if (task.status !== 'processing') {
+      throw new Error('Can only update progress on processing tasks');
+    }
+
+    task.progress = progress;
+    task.progressMessage = message;
+    task.updatedAt = new Date();
+
+    const listeners = this.progressListeners.get(taskId);
+    if (listeners) {
+      for (const listener of listeners.values()) {
+        listener(progress, message);
+      }
+    }
+  }
+
+  getProgress(taskId: string): { progress: number; message?: string } {
+    const task = this.tasks.get(taskId);
+    if (!task) throw new Error(`Task ${taskId} not found`);
+    return { progress: task.progress, message: task.progressMessage };
+  }
+
+  onProgress(taskId: string, listener: ProgressListener): () => void {
+    if (!this.progressListeners.has(taskId)) {
+      this.progressListeners.set(taskId, new Map());
+    }
+    const id = this.nextListenerId++;
+    this.progressListeners.get(taskId)!.set(id, listener);
+
+    return () => {
+      this.progressListeners.get(taskId)?.delete(id);
+    };
+  }
+
+  // â”€â”€ Phase 9: Cancellation â”€â”€
+
+  cancel(taskId: string): void {
+    const task = this.tasks.get(taskId);
+    if (!task) throw new Error(`Task ${taskId} not found`);
+    if (task.status !== 'pending' && task.status !== 'processing') {
+      throw new Error(`Cannot cancel task in ${task.status} state`);
+    }
+
+    task.status = 'cancelled';
+    task.updatedAt = new Date();
+  }
+
+  // â”€â”€ Phase 10: Recurring tasks â”€â”€
+
+  addSchedule(
+    name: string,
+    cron: string,
+    queueName: string,
+    payload: unknown,
+    options?: Omit<TaskOptions, 'queue'>,
+  ): void {
+    // Validate cron expression
+    parseCron(cron);
+
+    this.schedules.set(name, {
+      name,
+      cron,
+      queue: queueName,
+      payload,
+      options,
+    });
+  }
+
+  removeSchedule(name: string): void {
+    this.schedules.delete(name);
+  }
+
+  listSchedules(): Schedule[] {
+    return [...this.schedules.values()];
+  }
+
+  tick(now?: Date): void {
+    const currentTime = now ?? new Date();
+
+    for (const schedule of this.schedules.values()) {
+      const cronExpr = parseCron(schedule.cron);
+      if (cronExpr.matches(currentTime)) {
+        // Avoid duplicate firing in the same minute
+        if (schedule.lastRun) {
+          const lastMinute = Math.floor(
+            schedule.lastRun.getTime() / 60000,
+          );
+          const currentMinute = Math.floor(currentTime.getTime() / 60000);
+          if (lastMinute === currentMinute) continue;
+        }
+
+        schedule.lastRun = currentTime;
+        this.add(schedule.payload, {
+          ...schedule.options,
+          queue: schedule.queue,
+        });
+      }
+    }
+  }
+
+  // â”€â”€ Phase 11: Middleware â”€â”€
+
+  use(queueName: string, middleware: MiddlewareFn): void {
+    const state = this.getOrCreateQueue(queueName);
+    state.middlewares.push(middleware);
+  }
+
+  process(queueName: string, handler: HandlerFn): void {
+    const state = this.getOrCreateQueue(queueName);
+    state.handler = handler;
+  }
+
+  async processNext(queueName?: string): Promise<Task | null> {
+    const task = this.next(queueName);
+    if (!task) return null;
+
+    const queue = task.queue;
+    const state = this.queueStates.get(queue);
+    const middlewares = state?.middlewares ?? [];
+    const handler = state?.handler;
+
+    try {
+      let index = 0;
+      const executeNext = async (): Promise<void> => {
+        if (index < middlewares.length) {
+          const mw = middlewares[index++];
+          await mw(task, executeNext);
+        } else if (handler) {
+          await handler(task);
+        }
+      };
+
+      await executeNext();
+
+      // Auto-complete if still in processing state
+      if (task.status === 'processing') {
+        this.complete(task.id);
+      }
+    } catch (err) {
+      if (task.status === 'processing') {
+        this.fail(
+          task.id,
+          err instanceof Error ? err.message : String(err),
+        );
+      }
+    }
+
+    return task;
+  }
+
+  // â”€â”€ Phase 12: Graceful shutdown â”€â”€
+
+  onShutdown(callback: () => void | Promise<void>): void {
+    this.shutdownCallbacks.push(callback);
+  }
+
+  async shutdown(options?: ShutdownOptions): Promise<ShutdownResult> {
+    if (this.shutdownPromise) return this.shutdownPromise;
+
+    this.shuttingDown = true;
+    this.shutdownPromise = this.performShutdown(options);
+    return this.shutdownPromise;
+  }
+
+  private async performShutdown(
+    options?: ShutdownOptions,
+  ): Promise<ShutdownResult> {
+    const timeout = options?.timeout ?? 5000;
+    const start = Date.now();
+
+    // Run shutdown callbacks
+    for (const cb of this.shutdownCallbacks) {
+      try {
+        await cb();
+      } catch {
+        // ignore callback errors
+      }
+    }
+
+    // Get currently processing tasks
+    const processingIds = new Set<string>();
+    for (const task of this.tasks.values()) {
+      if (task.status === 'processing') {
+        processingIds.add(task.id);
+      }
+    }
+
+    if (processingIds.size === 0) {
+      return { completed: 0, forceCancelled: 0, duration: Date.now() - start };
+    }
+
+    // Wait for processing tasks to finish (up to timeout)
+    const deadline = start + timeout;
+
+    while (Date.now() < deadline) {
+      let anyStillProcessing = false;
+      for (const id of processingIds) {
+        const task = this.tasks.get(id);
+        if (task && task.status === 'processing') {
+          anyStillProcessing = true;
+          break;
+        }
+      }
+      if (!anyStillProcessing) break;
+      await new Promise((r) => setTimeout(r, 10));
+    }
+
+    // Force cancel remaining processing tasks
+    let forceCancelled = 0;
+    for (const id of processingIds) {
+      const task = this.tasks.get(id);
+      if (task && task.status === 'processing') {
+        task.status = 'cancelled';
+        task.updatedAt = new Date();
+        forceCancelled++;
+      }
+    }
+
+    const completed = processingIds.size - forceCancelled;
+
+    return {
+      completed,
+      forceCancelled,
+      duration: Date.now() - start,
+    };
+  }
+}
