+beforeEach(() => {
+  tq = new TaskQueue();
+});
+
+// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
+// Phase 1: Basic FIFO Queue
+// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
+
+describe('Phase 1: Basic FIFO Queue', () => {
+  it('should add a task and return a task object with required fields', () => {
+    const task = tq.add({ message: 'hello' });
+    expect(task.id).toBeDefined();
+    expect(task.status).toBe('pending');
+    expect(task.payload).toEqual({ message: 'hello' });
+    expect(task.createdAt).toBeInstanceOf(Date);
+  });
+
+  it('should assign unique IDs to each task', () => {
+    const t1 = tq.add('a');
+    const t2 = tq.add('b');
+    expect(t1.id).not.toBe(t2.id);
+  });
+
+  it('should retrieve tasks in FIFO order', () => {
+    tq.add('first');
+    tq.add('second');
+    tq.add('third');
+
+    expect(tq.next()!.payload).toBe('first');
+    expect(tq.next()!.payload).toBe('second');
+    expect(tq.next()!.payload).toBe('third');
+  });
+
+  it('should transition task to processing on retrieval', () => {
+    const added = tq.add('task');
+    const retrieved = tq.next()!;
+    expect(retrieved.id).toBe(added.id);
+    expect(retrieved.status).toBe('processing');
+  });
+
+  it('should return null when no tasks are available', () => {
+    expect(tq.next()).toBeNull();
+  });
+
+  it('should report queue size', () => {
+    expect(tq.size()).toBe(0);
+    tq.add('a');
+    tq.add('b');
+    expect(tq.size()).toBe(2);
+    tq.next();
+    expect(tq.size()).toBe(1);
+  });
+
+  it('should not return processing tasks again', () => {
+    tq.add('only');
+    tq.next();
+    expect(tq.next()).toBeNull();
+  });
+});
+
+// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
+// Phase 2: Named Queues
+// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
+
+describe('Phase 2: Named Queues', () => {
+  it('should support adding to named queues', () => {
+    tq.add('email', { queue: 'emails' });
+    tq.add('sms', { queue: 'sms' });
+
+    expect(tq.next('emails')!.payload).toBe('email');
+    expect(tq.next('sms')!.payload).toBe('sms');
+  });
+
+  it('should use default queue when name is omitted', () => {
+    tq.add('default-task');
+    expect(tq.next()!.payload).toBe('default-task');
+  });
+
+  it('should keep queues independent', () => {
+    tq.add('a', { queue: 'q1' });
+    tq.add('b', { queue: 'q2' });
+    expect(tq.size('q1')).toBe(1);
+    expect(tq.size('q2')).toBe(1);
+    expect(tq.next('q1')!.payload).toBe('a');
+    expect(tq.next('q1')).toBeNull();
+    expect(tq.next('q2')!.payload).toBe('b');
+  });
+
+  it('should list all queue names', () => {
+    tq.add('a', { queue: 'alpha' });
+    tq.add('b', { queue: 'beta' });
+    const names = tq.listQueues();
+    expect(names).toContain('alpha');
+    expect(names).toContain('beta');
+  });
+
+  it('should return null for non-existent queue', () => {
+    expect(tq.next('nonexistent')).toBeNull();
+  });
