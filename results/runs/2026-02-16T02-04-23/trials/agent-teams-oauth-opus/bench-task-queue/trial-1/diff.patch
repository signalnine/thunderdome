+    tq.use('default', async () => {
+      // intentionally not calling next
+    });
+    tq.process('default', async () => {
+      handlerCalled();
+    });
+
+    tq.add('x');
+    const task = await tq.processNext();
+
+    expect(handlerCalled).not.toHaveBeenCalled();
