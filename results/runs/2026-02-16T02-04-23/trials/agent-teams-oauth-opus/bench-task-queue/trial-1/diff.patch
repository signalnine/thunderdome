+describe('Integration', () => {
+  it('should handle priority + delay + dependencies together', () => {
+    vi.useFakeTimers();
+    try {
+      const dep = tq.add('dep', { priority: 5 });
+      tq.add('high-pri-blocked', { priority: 1, dependencies: [dep.id] });
+      tq.add('low-pri-available', { priority: 10 });
+
+      // dep and low-pri-available are available; dep has higher priority
+      const first = tq.next()!;
+      expect(first.payload).toBe('dep');
+
+      // low-pri-available is next (high-pri is still blocked)
+      const second = tq.next()!;
+      expect(second.payload).toBe('low-pri-available');
+
+      // high-pri-blocked is still blocked
+      expect(tq.next()).toBeNull();
+
+      // Complete dep
+      tq.complete(first.id);
+
+      // Now high-pri-blocked should be available
+      const third = tq.next()!;
+      expect(third.payload).toBe('high-pri-blocked');
+    } finally {
+      vi.useRealTimers();
+    }
+  });
+
+  it('should handle concurrency + priority correctly', () => {
+    tq.setConcurrency('default', 1);
