+export type TaskStatus = 'pending' | 'processing' | 'completed' | 'failed' | 'cancelled';
+
+export interface TaskOptions {
+  queue?: string;
+  priority?: number;
+  delay?: number;
+  availableAt?: Date;
+  maxRetries?: number;
+  backoffDelay?: number;
+  dependencies?: string[];
+}
+
+export interface Task {
+  id: string;
+  payload: unknown;
+  status: TaskStatus;
+  priority: number;
+  queue: string;
+  createdAt: Date;
+  updatedAt: Date;
+  availableAt: Date;
+  attempts: number;
+  maxRetries: number;
+  backoffDelay: number;
+  error?: string;
+  dependencies: string[];
+  progress: number;
+  progressMessage?: string;
+}
+
+export type MiddlewareFn = (task: Task, next: () => Promise<void>) => Promise<void>;
+export type HandlerFn = (task: Task) => Promise<void> | void;
+export type ProgressListener = (progress: number, message?: string) => void;
+
+export interface Schedule {
+  name: string;
+  cron: string;
+  queue: string;
+  payload: unknown;
+  options?: Omit<TaskOptions, 'queue'>;
+  lastRun?: Date;
+}
+
+export interface ShutdownOptions {
+  timeout?: number;
+}
+
+export interface ShutdownResult {
+  completed: number;
+  forceCancelled: number;
+  duration: number;
+}
