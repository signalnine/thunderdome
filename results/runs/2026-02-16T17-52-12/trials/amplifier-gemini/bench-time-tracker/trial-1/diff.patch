@@ -0,0 +1,135 @@
+
+import fs from 'node:fs/promises';
+import path from 'node:path';
+import { TimeEntry, TrackerData, TimeSummary } from './types.js';
+
+export class TimeTracker {
+  private filePath: string;
+  private data: TrackerData;
+  private loaded: boolean = false;
+
+  constructor(filePath: string) {
+    this.filePath = filePath;
+    this.data = {
+      activeEntry: null,
+      entries: []
+    };
+  }
+
+  private async load(): Promise<void> {
+    if (this.loaded) return;
+    try {
+      const content = await fs.readFile(this.filePath, 'utf-8');
+      this.data = JSON.parse(content);
+    } catch (error: unknown) {
+      if ((error as { code: string }).code === 'ENOENT') {
+        // File doesn't exist, start with empty data
+        this.data = {
+          activeEntry: null,
+          entries: []
+        };
+      } else {
+        throw error;
+      }
+    }
+    this.loaded = true;
+  }
+
+  private async save(): Promise<void> {
+    const dir = path.dirname(this.filePath);
+    await fs.mkdir(dir, { recursive: true });
+    await fs.writeFile(this.filePath, JSON.stringify(this.data, null, 2), 'utf-8');
+  }
+
+  async start(task: string): Promise<void> {
+    await this.load();
+
+    if (!task || task.trim().length === 0) {
+      throw new Error('Task name cannot be empty');
+    }
+
+    if (this.data.activeEntry) {
+      await this.stop();
+    }
+
+    this.data.activeEntry = {
+      task: task,
+      startTime: Date.now()
+    };
+
+    await this.save();
+  }
+
+  async stop(): Promise<void> {
+    await this.load();
+
+    if (!this.data.activeEntry) {
+      throw new Error('No timer is currently running');
+    }
+
+    const endTime = Date.now();
+    const newEntry: TimeEntry = {
+      task: this.data.activeEntry.task,
+      startTime: this.data.activeEntry.startTime,
+      endTime: endTime
+    };
+
+    this.data.entries.push(newEntry);
+    this.data.activeEntry = null;
+
+    await this.save();
+  }
+
+  async getEntries(filter?: { start?: number; end?: number }): Promise<TimeEntry[]> {
+    await this.load();
+
+    const entries: TimeEntry[] = [...this.data.entries];
+    if (this.data.activeEntry) {
+      entries.push({
+        task: this.data.activeEntry.task,
+        startTime: this.data.activeEntry.startTime
+      });
+    }
+
+    if (!filter) {
+      return entries;
+    }
+
+    return entries.filter(e => {
+      if (filter.start !== undefined && e.startTime < filter.start) {
+        return false;
+      }
+      if (filter.end !== undefined) {
+        if (e.endTime === undefined) {
+          return false; // Active entries haven't ended yet
+        }
+        if (e.endTime > filter.end) {
+          return false;
+        }
+      }
+      return true;
+    });
+  }
+
+  async getSummary(): Promise<TimeSummary> {
+    await this.load();
+
+    const summary: TimeSummary = {};
+
+    // Process completed entries
+    for (const entry of this.data.entries) {
+      if (entry.endTime !== undefined) {
+        const duration = entry.endTime - entry.startTime;
+        summary[entry.task] = (summary[entry.task] || 0) + duration;
+      }
+    }
+
+    // Process active entry if exists
+    if (this.data.activeEntry) {
+      const duration = Date.now() - this.data.activeEntry.startTime;
+      summary[this.data.activeEntry.task] = (summary[this.data.activeEntry.task] || 0) + duration;
+    }
+
+    return summary;
+  }
+}
