+
+export interface TimeEntry {
+  task: string;
+  startTime: number;
+  endTime?: number;
+}
+
+export interface ActiveEntry {
+  task: string;
+  startTime: number;
+}
+
+export interface TrackerData {
+  activeEntry: ActiveEntry | null;
+  entries: TimeEntry[];
+}
+
+export interface TimeSummary {
+  [task: string]: number;
+}
