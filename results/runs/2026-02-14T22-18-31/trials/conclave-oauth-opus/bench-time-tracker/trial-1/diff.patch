me).not.toBeNull();\n    });\n\n    it(\"should throw on empty task name\", () => {\n      expect(() => tracker.start(\"\")).toThrow();\n    });\n\n    it(\"should throw on whitespace-only task name\", () => {\n      expect(() => tracker.start(\"   \")).toThrow();\n    });\n\n    it(\"should support unicode and emoji task names\", () => {\n      const entry = tracker.start(\"코딩 🚀\");\n      expect(entry.taskName).toBe(\"코딩 🚀\");\n    });\n\n    it(\"should support complex emoji sequences\", () => {\n      const entry = tracker.start(\"👨‍👩‍👧‍👦 family time\");\n      expect(entry.taskName).toBe(\"👨‍👩‍👧‍👦 family time\");\n    });\n  });\n\n  describe(\"stop\", () => {\n    it(\"should stop the running timer\", () => {\n      tracker.start(\"coding\");\n      const stopped = tracker.stop();\n      expect(stopped.endTime).not.toBeNull();\n      expect(stopped.taskName).toBe(\"coding\");\n    });\n\n    it(\"should throw when no timer is running\", () => {\n      expect(() => tracker.stop()).toThrow();\n    });\n\n    it(\"should throw when all timers are already stopped\", () => {\n      tracker.start(\"task\");\n      tracker.stop();\n      expect(() => tracker.stop()).toThrow();\n    });\n  });\n\n  describe(\"getEntries\", () => {\n    it(\"should return all entries\", () => {\n      tracker.start(\"task1\");\n      tracker.stop();\n      tracker.start(\"task2\");\n      tracker.stop();\n      expect(tracker.getEntries()).toHaveLength(2);\n    });\n\n    it(\"should return empty array when no entries\", () => {\n      expect(tracker.getEntries()).toEqual([]);\n    });\n\n    it(\"should filter by from date\", () => {\n      tracker.start(\"old\");\n      tracker.stop();\n\n      // Small delay to ensure different timestamps\n      const cutoff = new Date();\n\n      tracker.start(\"new\");\n      tracker.stop();\n\n      const filtered = tracker.getEntries({ from: cutoff });\n      expect(filtered.every((e) => new Date(e.startTime) >= cutoff)).toBe(true);\n    });\n\n    it(\"should filter by to date\", () => {\n      tracker.start(\"task1\");\n      tracker.stop();\n      const cutoff = new Date();\n      tracker.start(\"task2\");\n      tracker.stop();\n      const filtered = tracker.getEntries({ to: cutoff });\n      expect(filtered.every((e) => new Date(e.startTime) <= cutoff)).toBe(true);\n    });\n\n    it(\"should filter by both from and to date\", () => {\n      tracker.start(\"before\");\n      tracker.stop();\n      const from = new Date();\n      tracker.start(\"during\");\n      tracker.stop();\n      const to = new Date();\n      tracker.start(\"after\");\n      tracker.stop();\n\n      const filtered = tracker.getEntries({ from, to });\n      for (const entry of filtered) {\n        const startTime = new Date(entry.startTime);\n        expect(startTime >= from).toBe(true);\n        expect(startTime <= to).toBe(true);\n      }\n    });\n\n    it(\"should return a copy of entries, not the internal array\", () => {\n      tracker.start(\"task\");\n      tracker.stop();\n      const entries = tracker.getEntries();\n      entries.pop();\n      expect(tracker.getEntries()).toHaveLength(1);\n    });\n  });\n\n  describe(\"getSummary\", () => {\n    it(\"should return total ms per task name\", () => {\n      tracker.start(\"coding\");\n      tracker.stop();\n      const summary = tracker.getSummary();\n      expect(summary[\"coding\"]).toBeGreaterThanOrEqual(0);\n    });\n\n    it(\"should sum multiple entries with the same task name\", () => {\n      tracker.start(\"task\");\n      tracker.stop();\n      tracker.start(\"task\");\n      tracker.stop();\n      const summary = tracker.getSummary();\n      expect(typeof summary[\"task\"]).toBe(\"number\");\n    });\n\n    it(\"should use current time for running tasks\", () => {\n      tracker.start(\"running\");\n      const summary = tracker.getSummary();\n      expect(summary[\"running\"]).toBeGreaterThanOrEqual(0);\n    });\n\n    it(\"should return empty object when no entries\", () => {\n      expect(tracker.getSummary()).toEqual({});\n    });\n\n    it(\"should track separate tasks independently\", () => {\n      tracker.start(\"task-a\");\n      tracker.stop();\n      tracker.start(\"task-b\");\n      tracker.stop();\n      const summary = tracker.getSummary();\n      expect(\"task-a\" in summary).toBe(true);\n      expect(\"task-b\" in summary).toBe(true);\n    });\n  });\n\n  describe(\"persistence\", () => {\n    it(\"should persist entries across instances\", () => {\n      tracker.start(\"task1\");\n      tracker.stop();\n      const tracker2 = new TimeTracker(filePath);\n      expect(tracker2.getEntries()).toHaveLength(1);\n      expect(tracker2.getEntries()[0].taskName).toBe(\"task1\");\n    });\n\n    it(\"should create parent directories automatically\", () => {\n      const nestedPath = join(tmpDir, \"a\", \"b\", \"c\", \"data.json\");\n      const nested = new TimeTracker(nestedPath);\n      nested.start(\"test\");\n      nested.stop();\n      const reloaded = new TimeTracker(nestedPath);\n      expect(reloaded.getEntries()).toHaveLength(1);\n    });\n\n    it(\"should handle missing file gracefully\", () => {\n      const missing = join(tmpDir, \"nonexistent.json\");\n      const t = new TimeTracker(missing);\n      expect(t.getEntries()).toEqual([]);\n    });\n\n    it(\"should persist data as valid JSON\", () => {\n      tracker.start(\"task\");\n      tracker.stop();\n      const raw = readFileSync(filePath, \"utf-8\");\n      const data = JSON.parse(raw);\n      expect(data.entries).toHaveLength(1);\n      expect(data.entries[0].taskName).toBe(\"task\");\n    });\n\n    it(\"should handle corrupted file gracefully\", () => {\n      const { writeFileSync: wfs } = require(\"node:fs\") as typeof import(\"node:fs\");\n      wfs(filePath, \"not json\");\n      const t = new TimeTracker(filePath);\n      expect(t.getEntries()).toEqual([]);\n    });\n\n    it(\"should persist unicode task names correctly\", () => {\n      tracker.start(\"日本語テスト 🎌\");\n      tracker.stop();\n      const tracker2 = new TimeTracker(filePath);\n      expect(tracker2.getEntries()[0].taskName).toBe(\"日本語テスト 🎌\");\n    });\n  });\n});\n","structuredPatch":[{"oldStart":210,"oldLines":8,"newStart":210,"newLines":7,"lines":["     });"," ","     it(\"should handle corrupted file gracefully\", () => {","-      const { writeFileSync: wfs } = require(\"node:fs\") as typeof import(\"node:fs\");","-      wfs(filePath, \"not json\");","+      writeFileSync(filePath, \"not json\");","       const t = new TimeTracker(filePath);","       expect(t.getEntries()).toEqual([]);","     });"]}],"userModified":false,"replaceAll":false}}
