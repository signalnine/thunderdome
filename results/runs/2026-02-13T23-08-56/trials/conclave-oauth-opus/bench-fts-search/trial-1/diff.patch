\n    48→    }\n    49→  });\n    50→\n    51→  // POST /notes - Create note\n    52→  router.post('/notes', (req: Request, res: Response, next: NextFunction) => {\n    53→    try {\n    54→      const input = req.body as NoteInput;\n    55→\n    56→      if (!input.title || input.title.trim() === '') {\n    57→        throw new ValidationError('Title is required');\n    58→      }\n    59→      if (!input.body || input.body.trim() === '') {\n    60→        throw new ValidationError('Body is required');\n    61→      }\n    62→\n    63→      const tags = input.tags ? JSON.stringify(input.tags) : '[]';\n    64→      const stmt = db.prepare(`\n    65→        INSERT INTO notes (title, body, tags) VALUES (?, ?, ?)\n    66→      `);\n    67→      const result = stmt.run(input.title, input.body, tags);\n    68→\n    69→      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(result.lastInsertRowid) as Note;\n    70→      res.status(201).json({ note });\n    71→    } catch (err) {\n    72→      next(err);\n    73→    }\n    74→  });\n    75→\n    76→  // PUT /notes/:id - Update note\n    77→  router.put('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    78→    try {\n    79→      const id = parseInt(req.params.id);\n    80→      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n    81→\n    82→      if (!existing) {\n    83→        throw new NotFoundError('Note not found');\n    84→      }\n    85→\n    86→      const input = req.body as Partial<NoteInput>;\n    87→      const title = input.title ?? existing.title;\n    88→      const body = input.body ?? existing.body;\n    89→      const tags = input.tags ? JSON.stringify(input.tags) : existing.tags;\n    90→\n    91→      const stmt = db.prepare(`\n    92→        UPDATE notes SET title = ?, body = ?, tags = ?, updated_at = datetime('now')\n    93→        WHERE id = ?\n    94→      `);\n    95→      stmt.run(title, body, tags, id);\n    96→\n    97→      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note;\n    98→      res.json({ note });\n    99→    } catch (err) {\n   100→      next(err);\n   101→    }\n   102→  });\n   103→\n   104→  // DELETE /notes/:id - Delete note\n   105→  router.delete('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n   106→    try {\n   107→      const id = parseInt(req.params.id);\n   108→      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n   109→\n   110→      if (!existing) {\n   111→        throw new NotFoundError('Note not found');\n   112→      }\n   113→\n   114→      db.prepare('DELETE FROM notes WHERE id = ?').run(id);\n   115→      res.status(204).send();\n   116→    } catch (err) {\n   117→      next(err);\n   118→    }\n   119→  });\n   120→\n   121→  return router;\n   122→}\n   123→"}]},"parent_tool_use_id":"toolu_01VjNfdnFdEmfVLQEpgXWHRU","session_id":"efb228db-b979-4c37-8a5d-0b915eb4bc40","uuid":"76a3dccd-7a40-4629-b644-1bd6a9411ea6"}
