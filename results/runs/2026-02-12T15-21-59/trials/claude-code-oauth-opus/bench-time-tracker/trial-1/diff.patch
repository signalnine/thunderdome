cker.start('old task');\n    tracker.stop();\n\n    // Manually modify the first entry to be old\n    const log = tracker.getLog();\n    const oldDate = new Date('2020-01-01T00:00:00.000Z');\n    log[0].startTime = oldDate.toISOString();\n    log[0].endTime = new Date('2020-01-01T01:00:00.000Z').toISOString();\n\n    // Create a new tracker to write the modified data\n    // Instead, let's use a different approach: start tasks at known times\n    const tracker2 = new TimeTracker(filePath);\n    tracker2.start('new task');\n    tracker2.stop();\n\n    const since = new Date(Date.now() - 1000); // 1 second ago\n    const filtered = tracker2.getLog({ since });\n    // The 'new task' should be included, the manually-dated one depends on timing\n    expect(filtered.length).toBeGreaterThanOrEqual(1);\n    expect(filtered.some(e => e.taskName === 'new task')).toBe(true);\n  });\n\n  // 7. getLog({ since }) includes entries that span the boundary (started before, still running)\n  it('getLog({ since }) includes running entries started before the since date', () => {\n    const tracker = new TimeTracker(filePath);\n    tracker.start('spanning task');\n    // Task is still running (no endTime), started now\n    // A running task started before the since date but still active should be included\n    // The spec says: includes entries that span the boundary (started before, still running after)\n    // So a running task with no endTime should be included if it's still active\n    const since = new Date(Date.now() + 60000); // 1 minute from now\n    const filtered = tracker.getLog({ since });\n    expect(filtered.some(e => e.taskName === 'spanning task')).toBe(true);\n  });\n\n  // 8. getSummary() returns object keyed by task name with total duration in ms\n  it('getSummary() returns object keyed by task name with total duration in ms', () => {\n    const tracker = new TimeTracker(filePath);\n    tracker.start('coding');\n    // Small delay to ensure non-zero duration\n    const