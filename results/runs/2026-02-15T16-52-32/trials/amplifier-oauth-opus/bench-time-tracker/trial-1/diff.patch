+import { describe, it, expect, beforeEach, afterEach, vi } from "vitest";
+import { TimeTracker } from "../src/index.js";
+import type { TimeEntry } from "../src/index.js";
+import { existsSync, rmSync, mkdirSync, readFileSync } from "node:fs";
+import { join } from "node:path";
+import { tmpdir } from "node:os";
+
+function tempFile(name: string): string {
+  return join(tmpdir(), "time-tracker-tests", name + ".json");
+}
+
+function cleanDir(): void {
+  const dir = join(tmpdir(), "time-tracker-tests");
+  if (existsSync(dir)) {
+    rmSync(dir, { recursive: true });
+  }
+}
+
+describe("TimeTracker", () => {
+  beforeEach(() => {
+    cleanDir();
+  });
+
+  afterEach(() => {
+    cleanDir();
+  });
+
+  describe("start", () => {
+    it("should start a timer and return the entry", () => {
+      const tracker = new TimeTracker(tempFile("start-basic"));
+      const entry = tracker.start("coding");
+
+      expect(entry.task).toBe("coding");
+      expect(entry.startedAt).toBeTruthy();
+      expect(entry.stoppedAt).toBeNull();
+    });
+
+    it("should throw when task name is empty", () => {
+      const tracker = new TimeTracker(tempFile("start-empty"));
+      expect(() => tracker.start("")).toThrow("Task name must not be empty");
+    });
+
+    it("should throw when task name is only whitespace", () => {
+      const tracker = new TimeTracker(tempFile("start-whitespace"));
+      expect(() => tracker.start("   ")).toThrow("Task name must not be empty");
+    });
+
+    it("should auto-stop a running timer when starting a new one", () => {
+      const tracker = new TimeTracker(tempFile("start-autostop"));
+      tracker.start("task-a");
+      tracker.start("task-b");
+
+      const entries = tracker.getEntries();
+      expect(entries).toHaveLength(2);
+      expect(entries[0].task).toBe("task-a");
+      expect(entries[0].stoppedAt).not.toBeNull();
+      expect(entries[1].task).toBe("task-b");
+      expect(entries[1].stoppedAt).toBeNull();
+    });
+  });
+
+  describe("stop", () => {
+    it("should stop the running timer", () => {
+      const tracker = new TimeTracker(tempFile("stop-basic"));
+      tracker.start("coding");
+      const stopped = tracker.stop();
+
+      expect(stopped.task).toBe("coding");
+      expect(stopped.stoppedAt).not.toBeNull();
+    });
+
+    it("should throw when no timer is running", () => {
+      const tracker = new TimeTracker(tempFile("stop-none"));
+      expect(() => tracker.stop()).toThrow("No timer is currently running");
+    });
+
+    it("should throw when stopping after already stopped", () => {
+      const tracker = new TimeTracker(tempFile("stop-double"));
+      tracker.start("coding");
+      tracker.stop();
+      expect(() => tracker.stop()).toThrow("No timer is currently running");
+    });
+  });
+
+  describe("status", () => {
+    it("should return null when no timer is running", () => {
+      const tracker = new TimeTracker(tempFile("status-none"));
+      expect(tracker.status()).toBeNull();
+    });
+
+    it("should return the running entry", () => {
+      const tracker = new TimeTracker(tempFile("status-running"));
+      tracker.start("coding");
+      const status = tracker.status();
+
+      expect(status).not.toBeNull();
+      expect(status!.task).toBe("coding");
+      expect(status!.stoppedAt).toBeNull();
+    });
+
+    it("should return null after stopping", () => {
+      const tracker = new TimeTracker(tempFile("status-stopped"));
+      tracker.start("coding");
+      tracker.stop();
+      expect(tracker.status()).toBeNull();
+    });
+  });
+
+  describe("getEntries", () => {
+    it("should return all entries", () => {
+      const tracker = new TimeTracker(tempFile("entries-all"));
+      tracker.start("task-a");
+      tracker.stop();
+      tracker.start("task-b");
+      tracker.stop();
+
+      const entries = tracker.getEntries();
+      expect(entries).toHaveLength(2);
+      expect(entries[0].task).toBe("task-a");
+      expect(entries[1].task).toBe("task-b");
+    });
+
+    it("should return empty array when no entries", () => {
+      const tracker = new TimeTracker(tempFile("entries-empty"));
+      expect(tracker.getEntries()).toEqual([]);
+    });
+
+    it("should filter by date range (from)", () => {
+      vi.useFakeTimers();
+      try {
+        vi.setSystemTime(new Date("2025-01-01T10:00:00Z"));
+        const tracker = new TimeTracker(tempFile("entries-from"));
+        tracker.start("old-task");
+        vi.setSystemTime(new Date("2025-01-01T10:05:00Z"));
+        tracker.stop();
+
+        const afterFirst = new Date("2025-01-01T10:10:00Z");
+
+        vi.setSystemTime(new Date("2025-01-01T10:15:00Z"));
+        tracker.start("new-task");
+        vi.setSystemTime(new Date("2025-01-01T10:20:00Z"));
+        tracker.stop();
+
+        const entries = tracker.getEntries({ from: afterFirst });
+        expect(entries).toHaveLength(1);
