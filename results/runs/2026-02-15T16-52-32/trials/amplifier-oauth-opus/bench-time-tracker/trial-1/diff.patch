+import { readFileSync, writeFileSync, mkdirSync, existsSync } from "node:fs";
+import { dirname } from "node:path";
+import type { TimeEntry, TrackerData, TimeSummary } from "./types.js";
+
+/**
+ * TimeTracker manages named timers with persistence to a JSON file.
+ *
+ * Only one timer can be running at a time. Starting a new timer
+ * automatically stops the previous one.
+ */
+export class TimeTracker {
+  private entries: TimeEntry[];
+  private readonly filePath: string;
+
+  constructor(filePath: string) {
+    this.filePath = filePath;
+    this.entries = this.load();
+  }
+
+  /**
+   * Start a timer for the given task name.
+   * If another timer is already running, it is stopped first.
+   *
+   * @throws Error if the task name is empty or only whitespace.
+   */
+  start(task: string): TimeEntry {
+    if (!task || task.trim().length === 0) {
+      throw new Error("Task name must not be empty.");
+    }
+
+    // Auto-stop any running timer
+    const running = this.getRunningEntry();
+    if (running) {
+      running.stoppedAt = new Date().toISOString();
+    }
+
+    const entry: TimeEntry = {
+      task,
+      startedAt: new Date().toISOString(),
+      stoppedAt: null,
+    };
+
+    this.entries.push(entry);
+    this.save();
+    return entry;
+  }
+
+  /**
+   * Stop the currently running timer.
+   *
+   * @throws Error if no timer is currently running.
+   */
+  stop(): TimeEntry {
+    const running = this.getRunningEntry();
+    if (!running) {
+      throw new Error("No timer is currently running.");
+    }
+
+    running.stoppedAt = new Date().toISOString();
+    this.save();
+    return running;
+  }
+
+  /**
+   * Get the currently running entry, or null if none.
+   */
+  status(): TimeEntry | null {
+    return this.getRunningEntry() ?? null;
+  }
+
+  /**
+   * Retrieve all entries, optionally filtered by a date range.
+   *
+   * Both `from` and `to` are inclusive. Only entries whose `startedAt`
+   * falls within the range are included.
+   */
+  getEntries(options?: { from?: Date; to?: Date }): TimeEntry[] {
+    if (!options?.from && !options?.to) {
+      return [...this.entries];
+    }
+
+    return this.entries.filter((entry) => {
+      const started = new Date(entry.startedAt).getTime();
+      if (options?.from && started < options.from.getTime()) {
+        return false;
+      }
+      if (options?.to && started > options.to.getTime()) {
+        return false;
+      }
+      return true;
+    });
+  }
+
+  /**
+   * Get a summary of total time spent per task name, in milliseconds.
+   *
+   * Running timers use the current time for their duration calculation.
+   * Multiple entries with the same task name are summed together.
+   */
+  summary(options?: { from?: Date; to?: Date }): TimeSummary {
+    const entries = this.getEntries(options);
+    const result: TimeSummary = {};
+
+    const now = Date.now();
+
+    for (const entry of entries) {
+      const start = new Date(entry.startedAt).getTime();
+      const end = entry.stoppedAt
+        ? new Date(entry.stoppedAt).getTime()
+        : now;
+      const duration = end - start;
+      result[entry.task] = (result[entry.task] ?? 0) + duration;
+    }
+
+    return result;
+  }
+
+  // --- Private helpers ---
+
+  private getRunningEntry(): TimeEntry | undefined {
+    return this.entries.find((e) => e.stoppedAt === null);
+  }
+
+  private load(): TimeEntry[] {
+    if (!existsSync(this.filePath)) {
+      return [];
+    }
+
+    try {
+      const raw = readFileSync(this.filePath, "utf-8");
+      const data = JSON.parse(raw) as TrackerData;
+      return data.entries ?? [];
+    } catch {
+      return [];
+    }
+  }
+
+  private save(): void {
+    const dir = dirname(this.filePath);
+    if (!existsSync(dir)) {
+      mkdirSync(dir, { recursive: true });
+    }
+
+    const data: TrackerData = { entries: this.entries };
+    writeFileSync(this.filePath, JSON.stringify(data, null, 2), "utf-8");
+  }
+}
