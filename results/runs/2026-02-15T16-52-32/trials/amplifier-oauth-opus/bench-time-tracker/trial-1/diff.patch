+        const entries = tracker.getEntries({ from, to });
+        expect(entries).toHaveLength(1);
+        expect(entries[0].task).toBe("during");
+      } finally {
+        vi.useRealTimers();
+      }
+    });
+
+    it("should return a copy, not a reference", () => {
+      const tracker = new TimeTracker(tempFile("entries-copy"));
+      tracker.start("task-a");
+      tracker.stop();
+
+      const entries = tracker.getEntries();
+      entries.length = 0;
+
+      expect(tracker.getEntries()).toHaveLength(1);
+    });
+  });
+
+  describe("summary", () => {
+    it("should sum durations per task name", () => {
+      const tracker = new TimeTracker(tempFile("summary-sum"));
+      tracker.start("coding");
+      tracker.stop();
+      tracker.start("coding");
+      tracker.stop();
+      tracker.start("review");
+      tracker.stop();
+
+      const s = tracker.summary();
+      expect(s["coding"]).toBeGreaterThanOrEqual(0);
+      expect(s["review"]).toBeGreaterThanOrEqual(0);
+      expect(Object.keys(s)).toHaveLength(2);
+    });
+
+    it("should include running timers using current time", () => {
+      const tracker = new TimeTracker(tempFile("summary-running"));
+      tracker.start("coding");
+      // Don't stop - should still appear in summary
+
+      const s = tracker.summary();
+      expect(s["coding"]).toBeGreaterThanOrEqual(0);
+    });
+
+    it("should return empty object when no entries", () => {
+      const tracker = new TimeTracker(tempFile("summary-empty"));
+      expect(tracker.summary()).toEqual({});
+    });
+
+    it("should support date range filtering", () => {
+      vi.useFakeTimers();
+      try {
+        vi.setSystemTime(new Date("2025-01-01T10:00:00Z"));
+        const tracker = new TimeTracker(tempFile("summary-range"));
