+/**
+ * A single time tracking entry.
+ */
+export interface TimeEntry {
+  /** The name of the task being tracked. */
+  task: string;
+  /** ISO 8601 timestamp of when the timer was started. */
+  startedAt: string;
+  /** ISO 8601 timestamp of when the timer was stopped, or null if still running. */
+  stoppedAt: string | null;
+}
+
+/**
+ * Persisted data format for the JSON file.
+ */
+export interface TrackerData {
+  entries: TimeEntry[];
+}
+
+/**
+ * A summary of total time spent per task name, in milliseconds.
+ */
+export type TimeSummary = Record<string, number>;
