+        vi.useRealTimers();
+      }
+    });
+
+    it("should filter by date range (to)", () => {
+      vi.useFakeTimers();
+      try {
+        vi.setSystemTime(new Date("2025-01-01T10:00:00Z"));
+        const tracker = new TimeTracker(tempFile("entries-to"));
+        tracker.start("first-task");
+        vi.setSystemTime(new Date("2025-01-01T10:05:00Z"));
+        tracker.stop();
+
+        const cutoff = new Date("2025-01-01T10:10:00Z");
+
+        vi.setSystemTime(new Date("2025-01-01T10:15:00Z"));
+        tracker.start("second-task");
+        vi.setSystemTime(new Date("2025-01-01T10:20:00Z"));
