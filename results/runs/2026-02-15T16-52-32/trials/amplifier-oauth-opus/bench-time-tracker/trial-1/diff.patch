+        vi.setSystemTime(new Date("2025-01-01T09:05:00Z"));
+        tracker.stop();
+
+        const from = new Date("2025-01-01T10:00:00Z");
+
+        vi.setSystemTime(new Date("2025-01-01T10:30:00Z"));
