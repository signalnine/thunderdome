;\n   118â†’    expect(summary).toHaveProperty('coding');\n   119â†’    expect(typeof summary['coding']).toBe('number');\n   120â†’    expect(summary['coding']).toBeGreaterThanOrEqual(0);\n   121â†’  });\n   122â†’\n   123â†’  // 9. getSummary() combines multiple entries for the same task name\n   124â†’  it('getSummary() combines multiple entries for the same task name', () => {\n   125â†’    const tracker = new TimeTracker(filePath);\n   126â†’    tracker.start('coding');\n   127â†’    const s1 = Date.now();\n   128â†’    while (Date.now() - s1 < 10) { /* busy wait */ }\n   129â†’    tracker.stop();\n   130â†’    tracker.start('coding');\n   131â†’    const s2 = Date.now();\n   132â†’    while (Date.now() - s2 < 10) { /* busy wait */ }\n   133â†’    tracker.stop();\n   134â†’    const summary = tracker.getSummary();\n   135â†’    expect(Object.keys(summary)).toHaveLength(1);\n   136â†’    expect(summary['coding']).toBeGreaterThanOrEqual(0);\n   137â†’  });\n   138â†’\n   139â†’  // 10. Persistence: save to file, create new instance with same file, data preserved\n   140â†’  it('persists data across instances', () => {\n   141â†’    const tracker1 = new TimeTracker(filePath);\n   142â†’    tracker1.start('persisted task');\n   143â†’    tracker1.stop();\n   144â†’\n   145â†’    const tracker2 = new TimeTracker(filePath);\n   146â†’    const log = tracker2.getLog();\n   147â†’    expect(log).toHaveLength(1);\n   148â†’    expect(log[0].taskName).toBe('persisted task');\n   149â†’  });\n   150â†’\n   151â†’  // 11. Empty name throws error\n   152â†’  it('start() with empty name throws an error', () => {\n   153â†’    const tracker = new TimeTracker(filePath);\n   154â†’    expect(() => tracker.start('')).toThrow();\n   155â†’  });\n   156â†’\n   157â†’  // 12. Unicode task names work\n   158â†’  it('supports unicode task names', () => {\n   159â†’    const tracker = new TimeTracker(filePath);\n   160â†’    const entry = tracker.start('ã‚³ãƒ¼ãƒ‡ã‚£ãƒ³ã‚° ðŸš€');\n   161â†’    expect(entry.taskName).toBe('ã‚³ãƒ¼ãƒ‡ã‚£ãƒ³ã‚° ðŸš€');\n   162â†’    tracker.stop();\n   163â†’    const log = tracker.getLog();\n   164â†’    expect(log[0].taskName).toBe('ã‚³ãƒ¼ãƒ‡ã‚£ãƒ³ã‚° ðŸš€');\n   165â†’  });\n   166â†’\n   167â†’  // 13. getActiveTask() returns current task or null\n   168â†’  it('getActiveTask() returns current task or null', () => {\n   169â†’    const tracker = new TimeTracker(filePath);\n   170â†’    expect(tracker.getActiveTask()).toBeNull();\n   171â†’    tracker.start('active task');\n   172â†’    const active = tracker.getActiveTask();\n   173â†’    expect(active).not.toBeNull();\n   174â†’    expect(active!.taskName).toBe('active task');\n   175â†’    tracker.stop();\n   176â†’    expect(tracker.getActiveTask()).toBeNull();\n   177â†’  });\n   178â†’\n   179â†’  // 14. Multiple start/stop cycles accumulate correctly\n   180â†’  it('multiple start/stop cycles accumulate correctly', () => {\n   181â†’    const tracker = new TimeTracker(filePath);\n   182â†’    tracker.start('task A');\n   183â†’    tracker.stop();\n   184â†’    tracker.start('task B');\n   185â†’    tracker.stop();\n   186â†’    tracker.start('task A');\n   187â†’    tracker.stop();\n   188â†’    const log = tracker.getLog();\n   189â†’    expect(log).toHaveLength(3);\n   190â†’    const summary = tracker.getSummary();\n   191â†’    expect(summary).toHaveProperty('task A');\n   192â†’    expect(summary).toHaveProperty('task B');\n   193â†’  });\n   194â†’\n   195â†’  // 15. Log entries have correct shape (taskName, startTime, endTime)\n   196â†’  it('log entries have correct shape', () => {\n   197â†’    const tracker = new TimeTracker(filePath);\n   198â†’    tracker.start('shaped task');\n   199â†’    tracker.stop();\n   200â†’    const log = tracker.getLog();\n   201â†’    const entry = log[0];\n   202â†’    expect(entry).toHaveProperty('taskName');\n   203â†’    expect(entry).toHaveProperty('startTime');\n   204â†’    expect(entry).toHaveProperty('endTim