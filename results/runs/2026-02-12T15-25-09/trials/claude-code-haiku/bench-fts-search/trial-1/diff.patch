x = index;\n    }\n  }\n\n  // Start snippet a bit before the match\n  snippetStart = Math.max(0, firstMatchIndex - 30);\n\n  // Extract snippet\n  let snippet = body.substring(snippetStart, snippetStart + 150);\n\n  // Add ellipsis if needed\n  if (snippetStart > 0) {\n    snippet = '...' + snippet;\n  }\n  if (snippetStart + 150 < body.length) {\n    snippet = snippet + '...';\n  }\n\n  // Highlight all matching terms with <mark> tags (case-insensitive)\n  for (const term of searchTerms) {\n    const cleanTerm = term.replace(/[^a-z0-9*]/gi, '');\n    const regex = new RegExp(`\\\\b${cleanTerm.replace(/\\*/g, '')}\\\\w*\\\\b`, 'gi');\n    snippet = snippet.replace(regex, (match) => `<mark>${match}</mark>`);\n  }\n\n  return snippet;\n}\n","structuredPatch":[{"oldStart":210,"oldLines":98,"newStart":210,"newLines":6,"lines":["     }","   });"," ","-  // GET /notes/search - Full-text search","-  router.get('/notes/search', (req: Request, res: Response, next: NextFunction) => {","-    try {","-      const q = req.query.q as string | undefined;","-      const page = Math.max(1, parseInt(req.query.page as string) || 1);","-      const perPage = 20;","-","-      if (!q || q.trim() === '') {","-        throw new ValidationError('Query parameter q is required and cannot be empty');","-      }","-","-      // Get total count and execute search with ranking","-      const countStmt = db.prepare(`","-        SELECT COUNT(*) as total FROM notes_fts WHERE notes_fts MATCH ?","-      `);","-      const countResult = countStmt.get(q) as { total: number } | undefined;","-      const total = countResult?.total || 0;","-","-      // Fetch results with pagination using BM25 ranking","-      const offset = (page - 1) * perPage;","-      const searchStmt = db.prepare(`","-        SELECT","-          notes.id,","-          notes.title,","-          notes.body,","-          notes_fts.rank","-        FROM notes_fts","-        JOIN notes ON notes.id = notes_fts.rowid","-        WHERE notes_f