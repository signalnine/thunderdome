;\n      if (tag) {\n        // Use a join with json_each for tag filtering\n        const tagStmt = db.prepare(`\n          SELECT DISTINCT notes.* FROM notes, json_each(notes.tags)\n          WHERE json_each.value = ?\n          ORDER BY created_at DESC\n        `);\n        notes = tagStmt.all(tag) as Note[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/:id - Get single note\n  router.get('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const stmt = db.prepare('SELECT * FROM notes WHERE id = ?');\n      const note = stmt.get(id) as Note | undefined;\n\n      if (!note) {\n        throw new NotFoundError('Note not found');\n      }\n\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // POST /notes - Create note\n  router.post('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const input = req.body as NoteInput;\n\n      if (!input.title || input.title.trim() === '') {\n        throw new ValidationError('Title is required');\n      }\n      if (!input.body || input.body.trim() === '') {\n        throw new ValidationError('Body is required');\n      }\n\n      const tags = input.tags ? JSON.stringify(input.tags) : '[]';\n      const stmt = db.prepare(`\n        INSERT INTO notes (title, body, tags) VALUES (?, ?, ?)\n      `);\n      const result = stmt.run(input.title, input.body, tags);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(result.lastInsertRowid) as Note;\n      res.status(201).json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // PUT /notes/:id - Update note\n  router.put('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      const input = req.body as Partial<NoteInput>;\n      const title = input.title ?? existing.title;\n      const body = input.body ?? existing.body;\n      const tags = input.tags ? JSON.stringify(input.tags) : existing.tags;\n\n      const stmt = db.prepare(`\n        UPDATE notes SET title = ?, body = ?, tags = ?, updated_at = datetime('now')\n        WHERE id = ?\n      `);\n      stmt.run(title, body, tags, id);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note;\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // DELETE /notes/:id - Delete note\n  router.delete('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      db.prepare('DELETE FROM notes WHERE id = ?').run(id);\n      res.status(204).send();\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  return router;\n}\n","numLines":123,"startLine":1,"totalLines":123}}}
