= req.query.q as string | undefined;\n      const page = Math.max(1, parseInt(req.query.page as string) || 1);\n      const perPage = 20;\n\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Query parameter q is required and cannot be empty');\n      }\n\n      // Sanitize FTS5 query to handle special characters\n      const sanitizedQuery = sanitizeFTSQuery(q);\n\n      // Get total count and execute search with ranking\n      const countStmt = db.prepare(`\n        SELECT COUNT(*) as total FROM notes_fts WHERE notes_fts MATCH ?\n      `);\n      const countResult = countStmt.get(sanitizedQuery) as { total: number } | undefined;\n      const total = countResult?.total || 0;\n\n      // Fetch results with pagination using BM25 ranking\n      const offset = (page - 1) * perPage;\n      const searchStmt = db.prepare(`\n        SELECT\n          notes.id,\n          notes.title,\n          notes.body,\n          notes_fts.rank\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        ORDER BY notes_fts.rank ASC\n        LIMIT ? OFFSET ?\n      `);\n\n      const rows = searchStmt.all(sanitizedQuery, perPage, offset) as Array<{\n        id: number;\n        title: string;\n        body: string;\n        rank: number;\n      }>;\n\n      // Create snippets with <mark> tags\n      const results: SearchResult[] = rows.map((row) => {\n        const snippet = createSnippet(row.body, q);\n        return {\n          id: row.id,\n          title: row.title,\n          snippet,\n          rank: row.rank,\n        };\n      });\n\n      res.json({\n        results,\n        total,\n        page,\n        perPage,\n      });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/search/suggest - Autocomplete suggestions (must come before /notes/:id)\n  router.get('/notes/search/suggest', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const q = req.query.q as string | undefined;\n\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Query parameter q is required and cannot be empty');\n      }\n\n      // Use prefix query with wildcard for autocomplete\n      const prefixQuery = q.trim() + '*';\n\n      const stmt = db.prepare(`\n        SELECT\n          notes.id,\n          notes.title\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        LIMIT 5\n      `);\n\n      const results = stmt.all(prefixQuery) as SuggestResult[];\n\n      res.json({ results });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes - List all notes (with optional tag filter)\n  router.get('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const tag = req.query.tag as string | undefined;\n\n      let notes: Note[];\n      if (tag) {\n        // Use a join with json_each for tag filtering\n        const tagStmt = db.prepare(`\n          SELECT DISTINCT notes.* FROM notes, json_each(notes.tags)\n          WHERE json_each.value = ?\n          ORDER BY created_at DESC\n        `);\n        notes = tagStmt.all(tag) as Note[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/:id - Get single note\n  router.get('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const stmt = db.prepare('SELECT * FROM notes WHERE id = ?');\n      const note = stmt.get(id) as Note | undefined;\n\n      if (!note) {\n        throw new NotFoundError('Note not found');\n      }\n\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // POST /notes - Create note\n  router.post('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const input = req.body as NoteInput;\n\n      if (!input.title || input.title.trim() === '') {\n        throw new ValidationError('Title is required');\n      }\n      if (!input.body || input.body.trim() === '') {\n        throw new ValidationError('Body is required');\n      }\n\n      const tags = input.tags ? JSON.stringify(input.tags) : '[]';\n      const stmt = db.prepare(`\n        INSERT INTO notes (title, body, tags) VALUES (?, ?, ?)\n      `);\n      const result = stmt.run(input.title, input.body, tags);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(result.lastInsertRowid) as Note;\n      res.status(201).json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // PUT /notes/:id - Update note\n  router.put('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      const input = req.body as Partial<NoteInput>;\n      const title = input.title ?? existing.title;\n      const body = input.body ?? existing.body;\n      const tags = input.tags ? JSON.stringify(input.tags) : existing.tags;\n\n      const stmt = db.prepare(`\n        UPDATE notes SET title = ?, body = ?, tags = ?, updated_at = datetime('now')\n        WHERE id = ?\n      `);\n      stmt.run(title, body, tags, id);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note;\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // DELETE /notes/:id - Delete note\n  router.delete('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n     