+// Helper function to create snippet with <mark> tags
+function createSnippet(body: string, query: string): string {
+  // Remove quotes from query for highlighting
+  const cleanQuery = query.trim().replace(/^"|"$/g, '');
+  const searchTerms = cleanQuery.toLowerCase().split(/\s+/);
+
+  // Find the first occurrence of any search term
+  let snippetStart = 0;
+  let firstMatchIndex = body.length;
+
+  for (const term of searchTerms) {
+    const cleanTerm = term.replace(/[^a-z0-9*]/gi, '');
+    const index = body.toLowerCase().indexOf(cleanTerm);
+    if (index !== -1 && index < firstMatchIndex) {
+      firstMatchIndex = index;
+    }
+  }
+
+  // Start snippet a bit before the match
+  snippetStart = Math.max(0, firstMatchIndex - 30);
+
+  // Extract snippet
+  let snippet = body.substring(snippetStart, snippetStart + 150);
+
+  // Add ellipsis if needed
+  if (snippetStart > 0) {
+    snippet = '...' + snippet;
+  }
+  if (snippetStart + 150 < body.length) {
+    snippet = snippet + '...';
+  }
+
+  // Highlight all matching terms with <mark> tags (case-insensitive)
+  for (const term of searchTerms) {
+    const cleanTerm = term.replace(/[^a-z0-9*]/gi, '');
+    const regex = new RegExp(`\\b${cleanTerm.replace(/\*/g, '')}\\w*\\b`, 'gi');
+    snippet = snippet.replace(regex, (match) => `<mark>${match}</mark>`);
+  }
