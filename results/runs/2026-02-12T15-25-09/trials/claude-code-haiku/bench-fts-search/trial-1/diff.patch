ex = body.length;\n\n  for (const term of searchTerms) {\n    const cleanTerm = term.replace(/[^a-z0-9*]/gi, '');\n    const index = body.toLowerCase().indexOf(cleanTerm);\n    if (index !== -1 && index < firstMatchIndex) {\n      firstMatchIndex = index;\n    }\n  }\n\n  // Start snippet a bit before the match\n  snippetStart = Math.max(0, firstMatchIndex - 30);\n\n  // Extract snippet\n  let snippet = body.substring(snippetStart, snippetStart + 150);\n\n  // Add ellipsis if needed\n  if (snippetStart > 0) {\n    snippet = '...' + snippet;\n  }\n  if (snippetStart + 150 < body.length) {\n    snippet = snippet + '...';\n  }\n\n  // Highlight all matching terms with <mark> tags (case-insensitive)\n  for (const term of searchTerms) {\n    const cleanTerm = term.replace(/[^a-z0-9*]/gi, '');\n    const regex = new RegExp(`\\\\b${cleanTerm.replace(/\\*/g, '')}\\\\w*\\\\b`, 'gi');\n    snippet = snippet.replace(regex, (match) => `<mark>${match}</mark>`);\n  }\n\n  return snippet;\n}\n","structuredPatch":[{"oldStart":218,"oldLines":14,"newStart":218,"newLines":15,"lines":[" "," // Helper function to sanitize FTS5 query"," function sanitizeFTSQuery(query: string): string {","-  // If query is wrapped in quotes (exact phrase), keep the quotes but escape inner quotes","+  // If query is wrapped in quotes (exact phrase), keep the quotes but escape special chars","   if (query.trim().startsWith('\"') && query.trim().endsWith('\"')) {","-    const phrase = query.trim().slice(1, -1).replace(/\"/g, '\"\"');","-    return `\"${phrase}\"`;","+    const phrase = query.trim().slice(1, -1);","+    // Remove all special FTS5 characters from the phrase","+    const cleaned = phrase.replace(/[:\"()&|*+\\-/<>{}[\\]'`@#$%^\\\\]/g, '');","+    return `\"${cleaned}\"`;","   }"," ","-  // For regular queries, escape special FTS5 characters by wrapping terms","-  // Split by spaces while preserving quoted phrases","+  // For regular queries, split by spaces while preserving quoted phrases","   