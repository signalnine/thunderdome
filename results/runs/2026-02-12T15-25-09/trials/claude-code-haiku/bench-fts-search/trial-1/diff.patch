 return {\n          id: row.id,\n          title: row.title,\n          snippet,\n          rank: row.rank,\n        };\n      });\n\n      res.json({\n        results,\n        total,\n        page,\n        perPage,\n      });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/search/suggest - Autocomplete suggestions (must come before /notes/:id)\n  router.get('/notes/search/suggest', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const q = req.query.q as string | undefined;\n\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Query parameter q is required and cannot be empty');\n      }\n\n      // Use prefix query with wildcard for autocomplete\n      const prefixQuery = q.trim() + '*';\n\n      const stmt = db.prepare(`\n        SELECT\n          notes.id,\n          notes.title\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        LIMIT 5\n      `);\n\n      const results = stmt.all(prefixQuery) as SuggestResult[];\n\n      res.json({ results });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes - List all notes (with optional tag filter)\n  router.get('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const tag = req.query.tag as string | undefined;\n\n      let notes: Note[];\n      if (tag) {\n        // Use a join with json_each for tag filtering\n        const tagStmt = db.prepare(`\n          SELECT DISTINCT notes.* FROM notes, json_each(notes.tags)\n          WHERE json_each.value = ?\n          ORDER BY created_at DESC\n        `);\n        notes = tagStmt.all(tag) as Note[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/:id - Get single note\n  router.get('/notes/:id', (