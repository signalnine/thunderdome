+// Helper function to sanitize FTS5 query
+function sanitizeFTSQuery(query: string): string {
+  // If query is wrapped in quotes (exact phrase), keep the quotes but escape special chars
+  if (query.trim().startsWith('"') && query.trim().endsWith('"')) {
+    const phrase = query.trim().slice(1, -1);
+    // Remove all special FTS5 characters from the phrase
+    const cleaned = phrase.replace(/[:"()&|*+\-/<>{}[\]'`@#$%^\\]/g, '');
+    return `"${cleaned}"`;
+  }
+
+  // For regular queries, split by spaces while preserving quoted phrases
+  const terms: string[] = [];
+  let current = '';
+  let inQuotes = false;
+
+  for (let i = 0; i < query.length; i++) {
+    const char = query[i];
+    if (char === '"') {
+      inQuotes = !inQuotes;
+      current += char;
+    } else if (char === ' ' && !inQuotes) {
+      if (current.trim()) {
+        terms.push(current.trim());
+      }
+      current = '';
+    } else {
+      current += char;
+    }
+  }
+
+  if (current.trim()) {
+    terms.push(current.trim());
+  }
+
+  // Process each term to remove special FTS5 characters
+  const sanitizedTerms = terms
+    .map((term) => {
+      // If it's a quoted phrase, clean it
+      if (term.startsWith('"') && term.endsWith('"')) {
+        const phrase = term.slice(1, -1).replace(/[:"()&|*+\-/<>{}[\]'`@#$%^\\]/g, '');
+        return phrase ? `"${phrase}"` : '';
+      }
+      // For regular terms, remove special FTS5 characters
+      return term.replace(/[:"()&|*+\-/<>{}[\]'`@#$%^\\]/g, '');
+    })
+    .filter((term) => term.length > 0);
+
+  return sanitizedTerms.length > 0 ? sanitizedTerms.join(' ') : '*';
+}
