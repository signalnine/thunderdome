+import Database from "better-sqlite3";
+import { readFileSync } from "fs";
+import { join } from "path";
+
+let db: Database.Database | null = null;
+
+export function getDb(dbPath?: string): Database.Database {
+  if (db) return db;
+  db = new Database(dbPath ?? ":memory:");
+  db.pragma("journal_mode = WAL");
+  db.pragma("foreign_keys = ON");
+  const schemaPath = join(__dirname, "..", "schema.sql");
+  try {
+    const schema = readFileSync(schemaPath, "utf-8");
+    db.exec(schema);
+  } catch {
+    // If schema.sql not found relative to dist, try cwd
+    const fallback = join(process.cwd(), "schema.sql");
+    const schema = readFileSync(fallback, "utf-8");
+    db.exec(schema);
+  }
+  return db;
+}
+
+export function resetDb(): void {
+  if (db) {
+    db.close();
+    db = null;
+  }
+}
+
+export function initDb(database: Database.Database): void {
+  db = database;
+}
