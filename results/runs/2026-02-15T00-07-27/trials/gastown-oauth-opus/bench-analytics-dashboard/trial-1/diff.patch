+import { Router, Request, Response } from "express";
+import { getDb } from "../db.js";
+import { authMiddleware } from "../auth.js";
+
+const router = Router();
+
+router.use(authMiddleware);
+
+interface QueryFilter {
+  event?: string;
+  startDate?: string;
+  endDate?: string;
+  properties?: Record<string, string>;
+}
+
+function buildWhereClause(tenantId: string, filter?: QueryFilter): { clause: string; params: (string | number)[] } {
+  let clause = "WHERE tenant_id = ?";
+  const params: (string | number)[] = [tenantId];
+
+  if (filter?.event) {
+    clause += " AND name = ?";
+    params.push(filter.event);
+  }
+
+  if (filter?.startDate) {
+    clause += " AND timestamp >= ?";
+    params.push(filter.startDate);
+  }
+
+  if (filter?.endDate) {
+    clause += " AND timestamp <= ?";
+    params.push(filter.endDate);
+  }
+
+  if (filter?.properties) {
+    for (const [key, value] of Object.entries(filter.properties)) {
+      clause += " AND json_extract(properties, ?) = ?";
+      params.push(`$.${key}`, value);
+    }
+  }
+
+  return { clause, params };
+}
+
+// POST /api/queries/count
+router.post("/count", (req: Request, res: Response): void => {
+  const db = getDb();
+  const tenantId = req.user!.tenantId;
+  const filter = req.body as QueryFilter;
+
+  const { clause, params } = buildWhereClause(tenantId, filter);
+  const result = db.prepare(`SELECT COUNT(*) as count FROM events ${clause}`).get(...params) as { count: number };
+
+  res.json({ count: result.count });
+});
+
+// POST /api/queries/timeseries
+router.post("/timeseries", (req: Request, res: Response): void => {
+  const db = getDb();
+  const tenantId = req.user!.tenantId;
+  const { interval, ...filter } = req.body as QueryFilter & { interval?: string };
+
+  const { clause, params } = buildWhereClause(tenantId, filter);
+
+  let dateFormat: string;
+  switch (interval) {
+    case "hour":
+      dateFormat = "%Y-%m-%dT%H:00:00";
+      break;
+    case "month":
+      dateFormat = "%Y-%m-01";
+      break;
+    case "week":
+      dateFormat = "%Y-W%W";
+      break;
+    default: // day
+      dateFormat = "%Y-%m-%d";
+      break;
+  }
+
+  const rows = db
+    .prepare(
+      `SELECT strftime('${dateFormat}', timestamp) as bucket, COUNT(*) as count FROM events ${clause} GROUP BY bucket ORDER BY bucket`
+    )
+    .all(...params) as Array<{ bucket: string; count: number }>;
+
+  res.json({ timeseries: rows });
+});
+
+// POST /api/queries/breakdown
+router.post("/breakdown", (req: Request, res: Response): void => {
+  const db = getDb();
+  const tenantId = req.user!.tenantId;
+  const { property, limit: queryLimit, ...filter } = req.body as QueryFilter & { property?: string; limit?: number };
+
+  if (!property) {
+    res.status(400).json({ error: "property is required for breakdown queries" });
+    return;
+  }
+
+  const { clause, params } = buildWhereClause(tenantId, filter);
+  const rowLimit = queryLimit || 50;
+
+  const rows = db
+    .prepare(
+      `SELECT json_extract(properties, ?) as value, COUNT(*) as count FROM events ${clause} AND json_extract(properties, ?) IS NOT NULL GROUP BY value ORDER BY count DESC LIMIT ?`
+    )
+    .all(`$.${property}`, ...params, `$.${property}`, rowLimit) as Array<{ value: string; count: number }>;
+
+  res.json({ breakdown: rows, property });
+});
+
+export default router;
