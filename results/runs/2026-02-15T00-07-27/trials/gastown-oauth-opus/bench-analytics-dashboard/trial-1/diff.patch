+    id: string;
+    type: string;
+    config: string;
+    position_x: number;
+    position_y: number;
+    width: number;
+    height: number;
+  }>;
+
+  const dashboard = {
+    id: row.id,
+    tenantId: row.tenant_id,
+    name: row.name,
+    ownerId: row.owner_id,
+    config: JSON.parse(row.config),
+    createdAt: row.created_at,
+    updatedAt: row.updated_at,
+    widgets: widgets.map((w) => ({
+      id: w.id,
+      type: w.type,
+      config: JSON.parse(w.config),
+      positionX: w.position_x,
+      positionY: w.position_y,
+      width: w.width,
+      height: w.height,
+    })),
+  };
+
+  if (format === "csv") {
+    const csv = dashboardToCsv(dashboard);
+    res.setHeader("Content-Type", "text/csv");
+    res.setHeader("Content-Disposition", `attachment; filename=dashboard-${row.id}.csv`);
+    res.send(csv);
+  } else {
+    res.json({ dashboard });
+  }
+});
+
+export default router;
