+    const gadgets = catalog.listProducts('gadgets');
+    expect(gadgets).toHaveLength(2);
+
+    const all = catalog.listProducts();
+    expect(all).toHaveLength(3);
+  });
+
+  it('should search products by name and category', () => {
+    const catalog = createCatalogService(bus);
+    catalog.createProduct('Blue Widget', 9.99, 'gadgets');
+    catalog.createProduct('Red Bolt', 1.50, 'hardware');
+
+    const results = catalog.searchProducts('widget');
