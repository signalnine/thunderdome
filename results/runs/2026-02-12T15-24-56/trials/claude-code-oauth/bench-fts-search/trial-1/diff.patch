nated results with BM25 ranking and snippets\n      const searchStmt = db.prepare(`\n        SELECT\n          notes.id,\n          notes.title,\n          snippet(notes_fts, 1, '<mark>', '</mark>', '...', 32) as snippet,\n          bm25(notes_fts) as rank\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        ORDER BY rank\n        LIMIT ? OFFSET ?\n      `);\n\n      const results = searchStmt.all(ftsQuery, perPage, offset) as SearchResult[];\n\n      res.json({ results, total, page, perPage });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes - List all notes (with optional tag filter)\n  router.get('/notes', (req: Request, res: Response, next: NextFunction) => {","originalFile":"import { Router, Request, Response, NextFunction } from 'express';\nimport Database from 'better-sqlite3';\nimport { Note, NoteInput } from './models.js';\nimport { NotFoundError, ValidationError } from './errors.js';\n\nexport function createRoutes(db: Database.Database): Router {\n  const router = Router();\n\n  // GET /notes - List all notes (with optional tag filter)\n  router.get('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const tag = req.query.tag as string | undefined;\n\n      let notes: Note[];\n      if (tag) {\n        // Use a join with json_each for tag filtering\n        const tagStmt = db.prepare(`\n          SELECT DISTINCT notes.* FROM notes, json_each(notes.tags)\n          WHERE json_each.value = ?\n          ORDER BY created_at DESC\n        `);\n        notes = tagStmt.all(tag) as Note[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/:id - Get single note\n  router.get('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\