qlite3';\nimport { Note, NoteInput, SearchResult, SuggestResult } from './models.js';\nimport { NotFoundError, ValidationError } from './errors.js';\n\nfunction sanitizeFtsQuery(query: string): string {\n  // If it looks like a quoted phrase, preserve it\n  if (query.startsWith('\"') && query.endsWith('\"')) {\n    // Escape any internal quotes and return as phrase query\n    const inner = query.slice(1, -1).replace(/\"/g, '');\n    return `\"${inner}\"`;\n  }\n  // For plain queries, strip FTS5 special characters and build a simple query\n  // Remove characters that have special meaning in FTS5\n  const cleaned = query.replace(/[*+\\-(){}[\\]^~<>:\"/\\\\&|!]/g, ' ').trim();\n  if (!cleaned) return '\"\"';\n  // Split into tokens and join - each token is searched individually\n  const tokens = cleaned.split(/\\s+/).filter(Boolean);\n  return tokens.join(' ');\n}\n\nexport function createRoutes(db: Database.Database): Router {\n  const router = Router();\n\n  // GET /notes/search/suggest?q=<prefix> - Autocomplete suggestions\n  router.get('/notes/search/suggest', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const q = req.query.q as string | undefined;\n\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Search query parameter \"q\" is required');\n      }\n\n      const prefix = q.trim();\n      const sanitized = sanitizeFtsQuery(prefix);\n\n      // Use prefix matching with FTS5 by appending *\n      let ftsQuery: string;\n      if (sanitized.startsWith('\"')) {\n        ftsQuery = sanitized;\n      } else {\n        const tokens = sanitized.split(/\\s+/).filter(Boolean);\n        // Make the last token a prefix query\n        const prefixTokens = tokens.map((t, i) => i === tokens.length - 1 ? `${t}*` : t);\n        ftsQuery = prefixTokens.join(' ');\n      }\n\n      const stmt = db.prepare(`\n        SELECT notes.id, notes.title\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        ORDER BY rank\n        LIMIT 5\n      `);\n\n      const results = stmt.all(ftsQuery) as SuggestResult[];\n      res.json({ results });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/search?q=<query> - Full-text search\n  router.get('/notes/search', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const q = req.query.q as string | undefined;\n\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Search query parameter \"q\" is required');\n      }\n\n      const page = Math.max(1, parseInt(req.query.page as string) || 1);\n      const perPage = 20;\n      const offset = (page - 1) * perPage;\n\n      const query = q.trim();\n      const ftsQuery = sanitizeFtsQuery(query);\n\n      // Count total results\n      const countStmt = db.prepare(`\n        SELECT COUNT(*) as total\n        FROM notes_fts\n        WHERE notes_fts MATCH ?\n      `);\n      const { total } = countStmt.get(ftsQuery) as { total: number };\n\n      // Get paginated results with BM25 ranking and snippets\n      const searchStmt = db.prepare(`\n        SELECT\n          notes.id,\n          notes.title,\n          snippet(notes_fts, 1, '<mark>', '</mark>', '...', 32) as snippet,\n          bm25(notes_fts) as rank\n        FROM notes_fts\n        JOIN notes ON notes.id = notes_fts.rowid\n        WHERE notes_fts MATCH ?\n        ORDER BY rank\n        LIMIT ? OFFSET ?\n      `);\n\n      const results = searchStmt.all(ftsQuery, perPage, offset) as SearchResult[];\n\n      res.json({ results, total, page, perPage });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes - List all notes (with optional tag filter)\n  router.get('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const tag = req.query.tag as string | undefined;\n\n      let notes: Note[];\n      if (tag) {\n        // Use a join with json_each for tag filtering\n        const tagStmt = db.prepare(`\n          SELECT DISTINCT notes.* FROM notes, json_each(notes.tags)\n          WHERE json_each.value = ?\n          ORDER BY created_at DESC\n        `);\n        notes = tagStmt.all(tag) as Note[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/:id - Get single note\n  router.get('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const stmt = db.prepare('SELECT * FROM notes WHERE id = ?');\n      const note = stmt.get(id) as Note | undefined;\n\n      if (!note) {\n        throw new NotFoundError('Note not found');\n      }\n\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // POST /notes - Create note\n  router.post('/notes', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const input = req.body as NoteInput;\n\n      if (!input.title || input.title.trim() === '') {\n        throw new ValidationError('Title is required');\n      }\n      if (!input.body || input.body.trim() === '') {\n        throw new ValidationError('Body is required');\n      }\n\n      const tags = input.tags ? JSON.stringify(input.tags) : '[]';\n      const stmt = db.prepare(`\n        INSERT INTO notes (title, body, tags) VALUES (?, ?, ?)\n      `);\n      const result = stmt.run(input.title, input.body, tags);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(result.lastInsertRowid) as Note;\n      res.status(201).json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // PUT /notes/:id - Update note\n  router.put('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      const input = req.body as Partial<NoteInput>;\n      const title = input.title ?? existing.title;\n      const body = input.body ?? existing.body;\n      const tags = input.tags ? JSON.stringify(input.tags) : existing.tags;\n\n      const stmt = db.prepare(`\n        UPDATE notes SET title = ?, body = ?, tags = ?, updated_at = datetime('now')\n        WHERE id = ?\n      `);\n      stmt.run(title, body, tags, id);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note;\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // DELETE /notes/:id - Delete note\n  router.delete('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      db.prepare('DELETE FROM notes WHERE id = ?').run(id);\n      res.status(204).send();\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  return router;\n}\n","structuredPatch":[{"oldStart":11,"oldLines":8,"newStart":11,"newLines":8,"lines":["     return `\"${inner}\"`;","   }","   // For plain queries, strip FTS5 special characters and build a simple query","-  // Remove characters that have special meaning in FTS5","-  const cleaned = query.replace(/[*+\\-(){}[\\]^~<>:\"/\\\\&|!]/g, ' ').trim();","+  // Remove characters that have special meaning in FTS5, plus quotes and apostrophes","+  const cleaned = query.replace(/[*+\\-(){}[\\]^~<>:\"/\\\\&|!']/g, ' ').trim();","   if (!cleaned) return '\"\"';","   // Split into tokens and join - each token is searched individually","   const tokens = cleaned.split(/\\s+/).filter(Boolean);"]}],"userModified":false,"replaceAll":false}}
