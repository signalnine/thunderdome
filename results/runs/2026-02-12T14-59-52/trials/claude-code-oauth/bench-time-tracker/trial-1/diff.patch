});\n   290â†’\n   291â†’  // 24. File is created if it doesn't exist\n   292â†’  it('creates the data file if it does not exist', () => {\n   293â†’    const newPath = join(tempDir, 'nonexistent.json');\n   294â†’    expect(existsSync(newPath)).toBe(false);\n   295â†’    const tracker = new TimeTracker(newPath);\n   296â†’    tracker.start('creation test');\n   297â†’    tracker.stop();\n   298â†’    expect(existsSync(newPath)).toBe(true);\n   299â†’  });\n   300â†’\n   301â†’  // 25. File path is configurable\n   302â†’  it('file path is configurable', () => {\n   303â†’    const customPath = join(tempDir, 'custom', 'path', 'data.json');\n   304â†’    const tracker = new TimeTracker(customPath);\n   305â†’    tracker.start('custom path task');\n   306â†’    tracker.stop();\n   307â†’    expect(existsSync(customPath)).toBe(true);\n   308â†’\n   309â†’    const tracker2 = new TimeTracker(customPath);\n   310â†’    const log = tracker2.getLog();\n   311â†’    expect(log).toHaveLength(1);\n   312â†’    expect(log[0].taskName).toBe('custom path task');\n   313â†’  });\n   314â†’});\n   315â†’"}]},"parent_tool_use_id":"toolu_01R2fzpwTfsJiNbSmCK9NyNT","session_id":"ea1e4153-a9cd-43be-a577-e1fcb7b4ebb9","uuid":"580bc9e0-0215-4634-80f5-7097e460ebfc"}
