+import { readFileSync, writeFileSync, mkdirSync, existsSync } from 'fs';
+import { dirname } from 'path';
+
+export interface Entry {
+  taskName: string;
+  startTime: string;
+  endTime: string | null;
+}
+
+export class TimeTracker {
+  private filePath: string;
+  private entries: Entry[] = [];
+  private activeEntry: Entry | null = null;
+
+  constructor(filePath: string) {
+    this.filePath = filePath;
+    this.loadFromFile();
+  }
+
+  private loadFromFile(): void {
+    if (existsSync(this.filePath)) {
+      try {
+        const data = readFileSync(this.filePath, 'utf-8');
+        this.entries = JSON.parse(data);
+        // Find the active entry (one with no endTime)
+        this.activeEntry = this.entries.find(e => e.endTime === null) || null;
+      } catch {
+        this.entries = [];
+        this.activeEntry = null;
+      }
+    }
+  }
+
+  private saveToFile(): void {
+    // Ensure parent directories exist
+    const dir = dirname(this.filePath);
+    if (!existsSync(dir)) {
+      mkdirSync(dir, { recursive: true });
+    }
+    writeFileSync(this.filePath, JSON.stringify(this.entries, null, 2));
+  }
+
+  start(taskName: string): Entry {
+    if (!taskName || taskName.trim() === '') {
+      throw new Error('Task name cannot be empty');
+    }
+
+    // Auto-stop the current active task if one is running
+    if (this.activeEntry) {
+      this.activeEntry.endTime = new Date().toISOString();
+    }
+
+    // Create new entry
+    const entry: Entry = {
+      taskName,
+      startTime: new Date().toISOString(),
+      endTime: null,
+    };
+
+    this.entries.push(entry);
+    this.activeEntry = entry;
+    this.saveToFile();
+
+    return entry;
+  }
+
+  stop(): Entry {
+    if (!this.activeEntry) {
+      throw new Error('No active task to stop');
+    }
+
+    this.activeEntry.endTime = new Date().toISOString();
+    const stoppedEntry = this.activeEntry;
+    this.activeEntry = null;
+    this.saveToFile();
+
+    return stoppedEntry;
+  }
+
+  getActiveTask(): Entry | null {
+    return this.activeEntry;
+  }
+
+  getLog(options?: { since?: Date }): Entry[] {
+    if (!options || !options.since) {
+      return this.entries;
+    }
+
+    const sinceTime = options.since.getTime();
+
+    return this.entries.filter(entry => {
+      const startTime = new Date(entry.startTime).getTime();
+      const endTime = entry.endTime ? new Date(entry.endTime).getTime() : null;
+
+      // Include if:
+      // 1. Started on or after the since date
+      if (startTime >= sinceTime) {
+        return true;
+      }
+
+      // 2. Still running (no endTime)
+      if (entry.endTime === null) {
+        return true;
+      }
+
+      // 3. Have an endTime on or after the since date (spanning the boundary)
+      if (endTime !== null && endTime >= sinceTime) {
+        return true;
+      }
+
+      return false;
+    });
+  }
+
+  getSummary(): Record<string, number> {
+    const summary: Record<string, number> = {};
+
+    for (const entry of this.entries) {
+      const startTime = new Date(entry.startTime).getTime();
+      let endTime: number;
+
+      if (entry.endTime === null) {
+        // For running tasks, use current time
+        endTime = Date.now();
+      } else {
+        endTime = new Date(entry.endTime).getTime();
+      }
+
+      const duration = endTime - startTime;
+
+      if (!summary[entry.taskName]) {
+        summary[entry.taskName] = 0;
+      }
+
+      summary[entry.taskName] += duration;
+    }
+
+    return summary;
+  }
+}
