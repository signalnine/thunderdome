onst input = req.body as InvoiceCreateInput;\n    45→    if (!input.customer_id || !input.date || !input.line_items?.length) {\n    46→      res.status(400).json({ error: 'customer_id, date, and line_items required' });\n    47→      return;\n    48→    }\n    49→    const id = generateInvoice(db, input);\n    50→    const invoice = getInvoiceById(db, id);\n    51→    res.status(201).json(invoice);\n    52→  });\n    53→\n    54→  // Monthly report\n    55→  app.get('/reports/monthly', (req: Request, res: Response) => {\n    56→    const year = parseInt(req.query.year as string, 10);\n    57→    if (isNaN(year)) {\n    58→      res.status(400).json({ error: 'year query parameter required' });\n    59→      return;\n    60→    }\n    61→    const report = getMonthlyReport(db, year);\n    62→    res.json(report);\n    63→  });\n    64→\n    65→  // Customer report\n    66→  app.get('/reports/customer/:id', (req: Request, res: Response) => {\n    67→    const customerId = req.params.id;\n    68→    const report = getCustomerReport(db, customerId);\n    69→    res.json(report);\n    70→  });\n    71→\n    72→  return { app, db };\n    73→}\n    74→\n    75→// Start server if run directly\n    76→const isMain = process.argv[1]?.endsWith('server.js') || process.argv[1]?.endsWith('server.ts');\n    77→if (isMain) {\n    78→  const { app } = createApp();\n    79→  const port = process.env.PORT || 3000;\n    80→  app.listen(port, () => {\n    81→    console.log(`Invoice service running on port ${port}`);\n    82→  });\n    83→}\n    84→"}]},"parent_tool_use_id":"toolu_01XfCre1UsBvRTrzATfi7yko","session_id":"7ef515c9-24b1-4fee-a410-bb141c41766c","uuid":"ac6e29ec-5ff3-433d-9111-4f2087573a90"}
