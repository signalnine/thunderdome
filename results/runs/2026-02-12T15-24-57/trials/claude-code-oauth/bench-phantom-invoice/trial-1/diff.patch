+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01U2rsMniU8bqWP4Xmombtcm","type":"tool_result","content":"     1→import type Database from 'better-sqlite3';\n     2→import type { InvoiceCreateInput, Invoice, LineItem } from '../models/invoice.js';\n     3→import { createInvoice } from '../db/queries.js';\n     4→\n     5→/**\n     6→ * Calculate the total for an invoice given subtotal, discount rate, and tax rate.\n     7→ *\n     8→ * Formula: subtotal * (1 - discountRate) * (1 + taxRate)\n     9→ *   - First apply the discount to get the discounted subtotal\n    10→ *   - Then apply tax on the discounted amount\n    11→ */\n    12→export function calculateInvoiceTotal(\n    13→  subtotal: number,\n    14→  discountRate: number,\n    15→  taxRate: number,\n    16→): number {\n    17→  const discounted = subtotal * (1 - discountRate);\n    18→  const total = discounted + subtotal * taxRate;\n    19→  return Math.round(total * 100) / 100;\n    20→}\n    21→\n    22→/**\n    23→ * Apply a discount to a subtotal.\n    24→ */\n    25→export function applyDiscount(subtotal: number, discountRate: number): number {\n    26→  return Math.round(subtotal * (1 - discountRate) * 100) / 100;\n    27→}\n    28→\n    29→/**\n    30→ * Calculate subtotal from line items.\n    31→ */\n    32→export function calculateSubtotal(lineItems: LineItem[]): number {\n    33→  return lineItems.reduce((sum, item) => sum + item.quantity * item.unit_price, 0);\n    34→}\n    35→\n    36→/**\n    37→ * Generate an invoice from input data, calculating subtotal and total.\n    38→ */\n    39→export function generateInvoice(\n    40→  db: Database.Database,\n    41→  input: InvoiceCreateInput,\n    42→): number {\n    43→  const subtotal = calculateSubtotal(input.line_items);\n    44→  const total = calculateInvoiceTotal(subtotal, input.discount_rate, input.tax_rate);\n    45→\n    46→  const invoice: Invoice & { line_items: LineItem[] } = {\n    47→    customer_id: input.customer_id,\n    48→    date: input.date,\n    49→    subtotal,\n    50→    discount_rate: input.discount_rate,\n    51→    tax_rate: input.tax_rate,\n    52→    total,\n    53→    status: input.status || 'draft',\n    54→    line_items: input.line_items,\n    55→  };\n    56→\n    57→  return createInvoice(db, invoice);\n    58→}\n    59→"}]},"parent_tool_use_id":"toolu_01XfCre1UsBvRTrzATfi7yko","session_id":"7ef515c9-24b1-4fee-a410-bb141c41766c","uuid":"6029ec6c-54b1-4027-b5e5-626c0244442e"}
