+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01RaewevFmLKETmiqGqPQ9Wz","type":"tool_result","content":"     1→import type Database from 'better-sqlite3';\n     2→import type { Invoice, LineItem } from '../models/invoice.js';\n     3→\n     4→/**\n     5→ * Retrieve invoices within a date range (inclusive on both ends).\n     6→ */\n     7→export function getInvoicesByDateRange(\n     8→  db: Database.Database,\n     9→  startDate: string,\n    10→  endDate: string,\n    11→): Invoice[] {\n    12→  const stmt = db.prepare(`\n    13→    SELECT * FROM invoices\n    14→    WHERE date >= ? AND date < ?\n    15→    ORDER BY date ASC\n    16→  `);\n    17→  return stmt.all(startDate, endDate) as Invoice[];\n    18→}\n    19→\n    20→/**\n    21→ * Retrieve a single invoice by ID, including its line items.\n    22→ */\n    23→export function getInvoiceById(\n    24→  db: Database.Database,\n    25→  id: number,\n    26→): (Invoice & { line_items: LineItem[] }) | undefined {\n    27→  const invoiceStmt = db.prepare('SELECT * FROM invoices WHERE id = ?');\n    28→  const invoice = invoiceStmt.get(id) as Invoice | undefined;\n    29→\n    30→  if (!invoice) return undefined;\n    31→\n    32→  const itemsStmt = db.prepare('SELECT * FROM line_items WHERE invoice_id = ?');\n    33→  const line_items = itemsStmt.all(id) as LineItem[];\n    34→\n    35→  return { ...invoice, line_items };\n    36→}\n    37→\n    38→/**\n    39→ * Create a new invoice with line items.\n    40→ */\n    41→export function createInvoice(\n    42→  db: Database.Database,\n    43→  data: Invoice & { line_items?: LineItem[] },\n    44→): number {\n    45→  const insertInvoice = db.prepare(`\n    46→    INSERT INTO invoices (customer_id, date, subtotal, discount_rate, tax_rate, total, status)\n    47→    VALUES (?, ?, ?, ?, ?, ?, ?)\n    48→  `);\n    49→\n    50→  const insertLineItem = db.prepare(`\n    51→    INSERT INTO line_items (invoice_id, description, quantity, unit_price)\n    52→    VALUES (?, ?, ?, ?)\n    53→  `);\n    54→\n    55→  const result = insertInvoice.run(\n    56→    data.customer_id,\n    57→    data.date,\n    58→    data.subtotal,\n    59→    data.discount_rate,\n    60→    data.tax_rate,\n    61→    data.total,\n    62→    data.status || 'draft',\n    63→  );\n    64→\n    65→  const invoiceId = result.lastInsertRowid as number;\n    66→\n    67→  if (data.line_items) {\n    68→    for (const item of data.line_items) {\n    69→      insertLineItem.run(invoiceId, item.description, item.quantity, item.unit_price);\n    70→    }\n    71→  }\n    72→\n    73→  return invoiceId;\n    74→}\n    75→\n    76→/**\n    77→ * Retrieve all invoices for a specific customer.\n    78→ */\n    79→export function getInvoicesByCustomer(\n    80→  db: Database.Database,\n    81→  customerId: string,\n    82→): Invoice[] {\n    83→  const stmt = db.prepare(`\n    84→    SELECT * FROM invoices\n    85→    WHERE customer_id = ?\n    86→    ORDER BY date ASC\n    87→  `);\n    88→  return stmt.all(customerId) as Invoice[];\n    89→}\n    90→"}]},"parent_tool_use_id":"toolu_01XfCre1UsBvRTrzATfi7yko","session_id":"7ef515c9-24b1-4fee-a410-bb141c41766c","uuid":"d1bf33fb-121e-4b56-842b-0b127c87a1f6"}
