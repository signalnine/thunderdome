+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_0132rz6ayvrzJoxAainwNB7K","type":"tool_result","content":"     1→// Force a non-UTC timezone so timezone-sensitive bugs are exposed\n     2→process.env.TZ = 'America/New_York';\n     3→\n     4→import { describe, it, expect, beforeEach } from 'vitest';\n     5→import type Database from 'better-sqlite3';\n     6→import { createDatabase } from '../src/db/connection.js';\n     7→import {\n     8→  getInvoicesByDateRange,\n     9→  getInvoiceById,\n    10→  createInvoice,\n    11→  getInvoicesByCustomer,\n    12→} from '../src/db/queries.js';\n    13→import {\n    14→  calculateInvoiceTotal,\n    15→  applyDiscount,\n    16→  calculateSubtotal,\n    17→  generateInvoice,\n    18→} from '../src/services/billing.js';\n    19→import { getMonthlyReport, getCustomerReport, getRevenueByPeriod } from '../src/services/reporting.js';\n    20→\n    21→let db: Database.Database;\n    22→\n    23→beforeEach(() => {\n    24→  db = createDatabase(':memory:');\n    25→});\n    26→\n    27→// ─── Invoice CRUD (8 tests) ────────────────────────────────────────\n    28→\n    29→describe('Invoice CRUD', () => {\n    30→  it('creates an invoice and returns its ID', () => {\n    31→    const id = createInvoice(db, {\n    32→      customer_id: 'cust-1',\n    33→      date: '2024-03-15',\n    34→      subtotal: 100,\n    35→      discount_rate: 0,\n    36→      tax_rate: 0.1,\n    37→      total: 110,\n    38→      status: 'draft',\n    39→    });\n    40→    expect(id).toBe(1);\n    41→  });\n    42→\n    43→  it('retrieves an invoice by ID', () => {\n    44→    const id = createInvoice(db, {\n    45→      customer_id: 'cust-1',\n    46→      date: '2024-03-15',\n    47→      subtotal: 200,\n    48→      discount_rate: 0.1,\n    49→      tax_rate: 0.08,\n    50→      total: 194.40,\n    51→      status: 'sent',\n    52→    });\n    53→    const invoice = getInvoiceById(db, id);\n    54→    expect(invoice).toBeDefined();\n    55→    expect(invoice!.customer_id).toBe('cust-1');\n    56→    expect(invoice!.subtotal).toBe(200);\n    57→  });\n    58→\n    59→  it('returns undefined for non-existent invoice', () => {\n    60→    const invoice = getInvoiceById(db, 999);\n    61→    expect(invoice).toBeUndefined();\n    62→  });\n    63→\n    64→  it('creates an invoice with line items', () => {\n    65→    const id = createInvoice(db, {\n    66→      customer_id: 'cust-2',\n    67→      date: '2024-04-01',\n    68→      subtotal: 250,\n    69→      discount_rate: 0,\n    70→      tax_rate: 0.05,\n    71→      total: 262.50,\n    72→      status: 'draft',\n    73→      line_items: [\n    74→        { description: 'Widget A', quantity: 5, unit_price: 30 },\n    75→        { description: 'Widget B', quantity: 2, unit_price: 50 },\n    76→      ],\n    77→    });\n    78→    const invoice = getInvoiceById(db, id);\n    79→    expect(invoice!.line_items).toHaveLength(2);\n    80→    expect(invoice!.line_items[0].description).toBe('Widget A');\n    81→  });\n    82→\n    83→  it('lists invoices by customer', () => {\n    84→    createInvoice(db, {\n    85→      customer_id: 'cust-A',\n    86→      date: '2024-01-10',\n    87→      subtotal: 100, discount_rate: 0, tax_rate: 0, total: 100, status: 'paid',\n    88→    });\n    89→    createInvoice(db, {\n    90→      customer_id: 'cust-B',\n    91→      date: '2024-01-12',\n    92→      subtotal: 200, discount_rate: 0, tax_rate: 0, total: 200, status: 'paid',\n    93→    });\n    94→    createInvoice(db, {\n    95→      customer_id: 'cust-A',\n    96→      date: '2024-02-05',\n    97→      subtotal: 150, discount_rate: 0, tax_rate: 0, total: 150, status: 'draft',\n    98→    });\n    99→\n   100→    const invoices = getInvoicesByCustomer(db, 'cust-A');\n   101→    expect(invoices).toHaveLength(2);\n   102→    expect(invoices.every(inv => inv.customer_id === 'cust-A')).toBe(true);\n   103→  });\n   104→\n   105→  it('returns invoices sorted by date', () => {\n   106→    createInvoice(db, {\n   107→      customer_id: 'cust-1',\n   108→      date: '2024-03-15',\n   109→      subtotal: 100, discount_rate: 0, tax_rate: 0, total: 100, status: 'draft',\n   110→    });\n   111→    createInvoice(db, {\n   112→      customer_id: 'cust-1',\n   113→      date: '2024-01-10',\n   114→      subtotal: 200, discount_rate: 0, tax_rate: 0, total: 200, status: 'draft',\n   115→    });\n   116→    createInvoice(db, {\n   117→      customer_id: 'cust-1',\n   118→      date: '2024-02-20',\n   119→      subtotal: 150, discount_rate: 0, tax_rate: 0, total: 150, status: 'draft',\n   120→    });\n   121→\n   122→    const invoices = getInvoicesByDateRange(db, '2024-01-01', '2024-12-31');\n   123→    expect(invoices[0].date).toBe('2024-01-10');\n   124→    expect(invoices[1].date).toBe('2024-02-20');\n   125→    expect(invoices[2].date).toBe('2024-03-15');\n   126→  });\n   127→\n   128→  it('stores invoice status correctly', () => {\n   129→    const id = createInvoice(db, {\n   130→      customer_id: 'cust-1',\n   131→      date: '2024-06-01',\n   132→      subtotal: 500, discount_rate: 0.05, tax_rate: 0.1, total: 522.50,\n   133→      status: 'paid',\n   134→    });\n   135→    const invoice = getInvoiceById(db, id);\n   136→    expect(invoice!.status).toBe('paid');\n   137→  });\n   138→\n   139→  it('calculates subtotal from line items', () => {\n   140→    const items = [\n   141→      { description: 'Item A', quantity: 3, unit_price: 10 },\n   142→      { description: 'Item B', quantity: 1, unit_price: 25.50 },\n   143→      { description: 'Item C', quantity: 2, unit_price: 7.25 },\n   144→    ];\n   145→    expect(calculateSubtotal(items)).toBe(70);\n   146→  });\n   147→});\n   148→\n   149→// ─── Billing Calculations (8 tests) ────────────────────────────────\n   150→\n   151→describe('Billing Calculations', () => {\n   152→  it('calculates total without discount', () => {\n   153→    // subtotal=100, no discount, 10% tax 