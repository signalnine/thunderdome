tCustomerReport(\n  db: Database.Database,\n  customerId: string,\n): CustomerReport {\n  const stmt = db.prepare(`\n    SELECT * FROM invoices\n    WHERE customer_id = ?\n  `);\n\n  const invoices = stmt.all(customerId) as Invoice[];\n\n  const totalSpent = invoices.reduce((sum, inv) => sum + inv.total, 0);\n\n  return {\n    customer_id: customerId,\n    invoice_count: invoices.length,\n    total_spent: Math.round(totalSpent * 100) / 100,\n    average_invoice: invoices.length > 0\n      ? Math.round((totalSpent / invoices.length) * 100) / 100\n      : 0,\n  };\n}\n\n/**\n * Get total revenue for a date period.\n */\nexport function getRevenueByPeriod(\n  db: Database.Database,\n  start: string,\n  end: string,\n): { total: number; count: number } {\n  const stmt = db.prepare(`\n    SELECT COALESCE(SUM(total), 0) as total, COUNT(*) as count\n    FROM invoices\n    WHERE date >= ? AND date <= ?\n  `);\n\n  const result = stmt.get(start, end) as { total: number; count: number };\n  return {\n    total: Math.round(result.total * 100) / 100,\n    count: result.count,\n  };\n}\n","structuredPatch":[{"oldStart":25,"oldLines":7,"newStart":25,"newLines":7,"lines":[" ","   for (const inv of invoices) {","     const d = new Date(inv.date + 'T00:00:00Z');","-    const month = d.getMonth() + 1; // 1-indexed","+    const month = d.getUTCMonth() + 1; // 1-indexed"," ","     const current = monthlyMap.get(month) || { count: 0, total: 0 };","     current.count += 1;"]}],"userModified":false,"replaceAll":false}}
