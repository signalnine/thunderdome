+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01GbLm2UczYYWVbsGLQVEcQD","type":"tool_result","content":"     1→import type Database from 'better-sqlite3';\n     2→import type { Invoice, InvoiceSummary, CustomerReport } from '../models/invoice.js';\n     3→\n     4→/**\n     5→ * Generate a monthly revenue report for a given year.\n     6→ * Groups invoices by their UTC month and aggregates totals.\n     7→ */\n     8→export function getMonthlyReport(\n     9→  db: Database.Database,\n    10→  year: number,\n    11→): InvoiceSummary[] {\n    12→  const startDate = `${year}-01-01`;\n    13→  const endDate = `${year}-12-31`;\n    14→\n    15→  const stmt = db.prepare(`\n    16→    SELECT * FROM invoices\n    17→    WHERE date >= ? AND date <= ?\n    18→    ORDER BY date ASC\n    19→  `);\n    20→\n    21→  const invoices = stmt.all(startDate, endDate) as Invoice[];\n    22→\n    23→  // Group invoices by month\n    24→  const monthlyMap = new Map<number, { count: number; total: number }>();\n    25→\n    26→  for (const inv of invoices) {\n    27→    const d = new Date(inv.date + 'T00:00:00Z');\n    28→    const month = d.getMonth() + 1; // 1-indexed\n    29→\n    30→    const current = monthlyMap.get(month) || { count: 0, total: 0 };\n    31→    current.count += 1;\n    32→    current.total += inv.total;\n    33→    monthlyMap.set(month, current);\n    34→  }\n    35→\n    36→  // Convert to array of summaries\n    37→  const summaries: InvoiceSummary[] = [];\n    38→  for (const [month, data] of monthlyMap) {\n    39→    summaries.push({\n    40→      month,\n    41→      year,\n    42→      count: data.count,\n    43→      total_revenue: Math.round(data.total * 100) / 100,\n    44→    });\n    45→  }\n    46→\n    47→  return summaries.sort((a, b) => a.month - b.month);\n    48→}\n    49→\n    50→/**\n    51→ * Generate a report for a specific customer.\n    52→ */\n    53→export function getCustomerReport(\n    54→  db: Database.Database,\n    55→  customerId: string,\n    56→): CustomerReport {\n    57→  const stmt = db.prepare(`\n    58→    SELECT * FROM invoices\n    59→    WHERE customer_id = ?\n    60→  `);\n    61→\n    62→  const invoices = stmt.all(customerId) as Invoice[];\n    63→\n    64→  const totalSpent = invoices.reduce((sum, inv) => sum + inv.total, 0);\n    65→\n    66→  return {\n    67→    customer_id: customerId,\n    68→    invoice_count: invoices.length,\n    69→    total_spent: Math.round(totalSpent * 100) / 100,\n    70→    average_invoice: invoices.length > 0\n    71→      ? Math.round((totalSpent / invoices.length) * 100) / 100\n    72→      : 0,\n    73→  };\n    74→}\n    75→\n    76→/**\n    77→ * Get total revenue for a date period.\n    78→ */\n    79→export function getRevenueByPeriod(\n    80→  db: Database.Database,\n    81→  start: string,\n    82→  end: string,\n    83→): { total: number; count: number } {\n    84→  const stmt = db.prepare(`\n    85→    SELECT COALESCE(SUM(total), 0) as total, COUNT(*) as count\n    86→    FROM invoices\n    87→    WHERE date >= ? AND date <= ?\n    88→  `);\n    89→\n    90→  const result = stmt.get(start, end) as { total: number; count: number };\n    91→  return {\n    92→    total: Math.round(result.total * 100) / 100,\n    93→    count: result.count,\n    94→  };\n    95→}\n    96→"}]},"parent_tool_use_id":"toolu_01XfCre1UsBvRTrzATfi7yko","session_id":"7ef515c9-24b1-4fee-a410-bb141c41766c","uuid":"b44821f1-2c1c-4b3c-b7bf-4a506b49b742"}
