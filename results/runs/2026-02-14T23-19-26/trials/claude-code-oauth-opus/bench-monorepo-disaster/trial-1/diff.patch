s.status(400).json({ errors: validation.errors });\n    60→      return;\n    61→    }\n    62→\n    63→    const user = createUser(req.body);\n    64→    res.status(201).json({ user });\n    65→  } catch (err) {\n    66→    if (err instanceof Error && err.message.includes('UNIQUE')) {\n    67→      res.status(409).json({ error: 'Email already exists' });\n    68→      return;\n    69→    }\n    70→    res.status(500).json({ error: 'Failed to create user' });\n    71→  }\n    72→});\n    73→\n    74→// Delete user by ID\n    75→usersRouter.delete('/:id', (req: Request, res: Response) => {\n    76→  try {\n    77→    const callerRole = (req.headers['x-user-role'] as Role) || 'viewer';\n    78→    if (!canManageUsers(callerRole)) {\n    79→      res.status(403).json({ error: 'Insufficient permissions' });\n    80→      return;\n    81→    }\n    82→\n    83→    const validation = validateUserId(req.params['id']!);\n    84→    if (!validation.success) {\n    85→      res.status(400).json({ errors: validation.errors });\n    86→      return;\n    87→    }\n    88→\n    89→    const user = getUserById(validation.data!);\n    90→    if (!user) {\n    91→      res.status(404).json({ error: 'User not found' });\n    92→      return;\n    93→    }\n    94→\n    95→    // In a real app, we'd delete here\n    96→    res.status(200).json({ message: 'User deleted', user });\n    97→  } catch (err) {\n    98→    res.status(500).json({ error: 'Failed to delete user' });\n    99→  }\n   100→});\n   101→"}]},"parent_tool_use_id":"toolu_01VPq861j2NeQexJgst44kHo","session_id":"74e7d7a6-4935-486f-9d4b-ab661c0fc474","uuid":"aca6e99e-e652-4711-842e-84812b6d7d3b"}
