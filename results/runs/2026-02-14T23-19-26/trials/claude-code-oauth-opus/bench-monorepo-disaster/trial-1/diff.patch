+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_017z5P3eWqwXA2BU3UhY6dBz","type":"tool_result","content":"     1→/**\n     2→ * Worker entry point.\n     3→ * Runs background jobs on a schedule.\n     4→ */\n     5→\n     6→import { Scheduler } from './scheduler.js';\n     7→import { syncJob } from './jobs/sync.js';\n     8→import { cleanupJob } from './jobs/cleanup.js';\n     9→\n    10→export function createWorker(): Scheduler {\n    11→  const scheduler = new Scheduler();\n    12→\n    13→  // Register jobs\n    14→  scheduler.register('sync', syncJob, 60_000); // Every 60 seconds\n    15→  scheduler.register('cleanup', async () => { await cleanupJob(); }, 300_000); // Every 5 minutes\n    16→\n    17→  return scheduler;\n    18→}\n    19→\n    20→// Start worker if run directly\n    21→const isMainModule = process.argv[1]?.includes('worker/dist/index.js');\n    22→if (isMainModule) {\n    23→  console.log('Starting worker...');\n    24→  const worker = createWorker();\n    25→  worker.start();\n    26→\n    27→  process.on('SIGINT', () => {\n    28→    console.log('Shutting down worker...');\n    29→    worker.stop();\n    30→    process.exit(0);\n    31→  });\n    32→}\n    33→"}]},"parent_tool_use_id":"toolu_01VPq861j2NeQexJgst44kHo","session_id":"74e7d7a6-4935-486f-9d4b-ab661c0fc474","uuid":"800c9d49-7244-434a-b92d-f5e5942df825"}
