+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_011NhM6sVCjuwZy3jdGEGT7K","type":"tool_result","content":"     1→/**\n     2→ * Permission checking and role-based access control.\n     3→ */\n     4→\n     5→import type { Role, Permission } from './types.js';\n     6→\n     7→const ROLE_PERMISSIONS: Record<Role, Permission[]> = {\n     8→  admin: ['read', 'write', 'delete', 'manage_users', 'view_reports'],\n     9→  user: ['read', 'write', 'view_reports'],\n    10→  viewer: ['read'],\n    11→};\n    12→\n    13→export function hasPermission(role: Role, permission: Permission): boolean {\n    14→  const permissions = ROLE_PERMISSIONS[role];\n    15→  return permissions.includes(permission);\n    16→}\n    17→\n    18→export function getAllPermissions(role: Role): Permission[] {\n    19→  return [...ROLE_PERMISSIONS[role]];\n    20→}\n    21→\n    22→export function canManageUsers(role: Role): boolean {\n    23→  return hasPermission(role, 'manage_users');\n    24→}\n    25→\n    26→export function canWrite(role: Role): boolean {\n    27→  return hasPermission(role, 'write');\n    28→}\n    29→\n    30→export function canDelete(role: Role): boolean {\n    31→  return hasPermission(role, 'delete');\n    32→}\n    33→\n    34→export function requirePermission(\n    35→  role: Role,\n    36→  permission: Permission\n    37→): void {\n    38→  if (!hasPermission(role, permission)) {\n    39→    throw new Error(\n    40→      `Role '${role}' does not have permission '${permission}'`\n    41→    );\n    42→  }\n    43→}\n    44→"}]},"parent_tool_use_id":"toolu_01VPq861j2NeQexJgst44kHo","session_id":"74e7d7a6-4935-486f-9d4b-ab661c0fc474","uuid":"9aed24e0-91f4-4a78-b64d-ae44584d53c6"}
