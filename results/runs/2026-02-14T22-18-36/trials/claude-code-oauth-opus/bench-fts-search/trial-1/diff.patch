nst result = stmt.run(input.title, input.body, tags);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(result.lastInsertRowid) as Note;\n      res.status(201).json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // PUT /notes/:id - Update note\n  router.put('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      const input = req.body as Partial<NoteInput>;\n      const title = input.title ?? existing.title;\n      const body = input.body ?? existing.body;\n      const tags = input.tags ? JSON.stringify(input.tags) : existing.tags;\n\n      const stmt = db.prepare(`\n        UPDATE notes SET title = ?, body = ?, tags = ?, updated_at = datetime('now')\n        WHERE id = ?\n      `);\n      stmt.run(title, body, tags, id);\n\n      const note = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note;\n      res.json({ note });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // DELETE /notes/:id - Delete note\n  router.delete('/notes/:id', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const id = parseInt(req.params.id);\n      const existing = db.prepare('SELECT * FROM notes WHERE id = ?').get(id) as Note | undefined;\n\n      if (!existing) {\n        throw new NotFoundError('Note not found');\n      }\n\n      db.prepare('DELETE FROM notes WHERE id = ?').run(id);\n      res.status(204).send();\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  return router;\n}\n","structuredPatch":[{"oldStart":206,"oldLines":3,"newStart":206,"newLines":25,"lines":[" ","   return router;"," }","+","+function sanitizeFtsQuery(query: string): string {","+  // Preserve quoted phrases","+  const phrases: string[] = [];","+  let cleane