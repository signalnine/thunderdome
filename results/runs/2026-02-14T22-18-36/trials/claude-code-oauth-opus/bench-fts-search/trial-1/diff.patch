te[];\n      } else {\n        const stmt = db.prepare('SELECT * FROM notes ORDER BY created_at DESC');\n        notes = stmt.all() as Note[];\n      }\n\n      res.json({ notes });\n    } catch (err) {\n      next(err);\n    }\n  });\n\n  // GET /notes/search?q=<query> - Full-text search\n  router.get('/notes/search', (req: Request, res: Response, next: NextFunction) => {\n    try {\n      const q = req.query.q as string | undefined;\n      if (!q || q.trim() === '') {\n        throw new ValidationError('Search query parameter \"q\" is required');\n      }\n\n      const page = Math.max(1, parseInt(req.query.page as string) || 1);\n      const perPage = 20;\n      const offset = (page - 1) * perPage;\n\n      // Sanitize the query for FTS5: escape special characters that aren't part of phrase queries\n      const sanitized = sanitizeFtsQuery(q);\n\n      // Count total results\n      const countStmt = db.prepare(`\n        SELECT COUNT(*) as total FROM notes_fts WHERE notes_fts MATCH ?\n      `);\n      let total: number;\n      try {\n        total = (countStmt.get(sanitized) as { total: number }).total;\n      } catch {\n        // If the query syntax is invalid for FTS5, return empty results\n        res.json({ results: [], total: 0, page, perPage });\n        return;\n      }\n\n      // Search with BM25 ranking and snippet highlighting\n      const searchStmt = db.prepare(`\n        SELECT\n          notes_fts.rowid as id,\n          notes_fts.title,\n          snippet(notes_fts, 1, '<mark>', '</mark>', '...', 32) as snippet,\n          bm25(notes_fts) as rank\n        FROM notes_fts\n        WHERE notes_fts MATCH ?\n        ORDER BY rank\n        LIMIT ? OFFSET ?\n      `);\n\n      let results: SearchResult[];\n      try {\n        results = searchStmt.all(sanitized, perPage, offset) as SearchResult[];\n      } catch {\n        res.json({ results: [], total: 0, page, perPage });\n        return;\n      }\n\n      res.json({ results, total, page, perPage });\