+function sanitizeFtsQuery(query: string): string {
+  // Preserve quoted phrases
+  const phrases: string[] = [];
+  let cleaned = query.replace(/"([^"]*)"/g, (_match, phrase) => {
+    phrases.push(`"${phrase}"`);
+    return `__PHRASE${phrases.length - 1}__`;
+  });
+
+  // Remove FTS5 special characters (except already-handled quotes)
+  cleaned = cleaned.replace(/[^a-zA-Z0-9_\s]/g, ' ');
+
+  // Restore quoted phrases
+  for (let i = 0; i < phrases.length; i++) {
+    cleaned = cleaned.replace(`__PHRASE${i}__`, phrases[i]);
+  }
+
+  // Collapse whitespace
+  cleaned = cleaned.replace(/\s+/g, ' ').trim();
+
