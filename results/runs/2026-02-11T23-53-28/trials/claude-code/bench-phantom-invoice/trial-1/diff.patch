ry, CustomerReport } from '../models/invoice.js';\n\n/**\n * Generate a monthly revenue report for a given year.\n * Groups invoices by their UTC month and aggregates totals.\n */\nexport function getMonthlyReport(\n  db: Database.Database,\n  year: number,\n): InvoiceSummary[] {\n  const startDate = `${year}-01-01`;\n  const endDate = `${year}-12-31`;\n\n  const stmt = db.prepare(`\n    SELECT * FROM invoices\n    WHERE date >= ? AND date <= ?\n    ORDER BY date ASC\n  `);\n\n  const invoices = stmt.all(startDate, endDate) as Invoice[];\n\n  // Group invoices by month\n  const monthlyMap = new Map<number, { count: number; total: number }>();\n\n  for (const inv of invoices) {\n    const d = new Date(inv.date + 'T00:00:00Z');\n    const month = d.getMonth() + 1; // 1-indexed\n\n    const current = monthlyMap.get(month) || { count: 0, total: 0 };\n    current.count += 1;\n    current.total += inv.total;\n    monthlyMap.set(month, current);\n  }\n\n  // Convert to array of summaries\n  const summaries: InvoiceSummary[] = [];\n  for (const [month, data] of monthlyMap) {\n    summaries.push({\n      month,\n      year,\n      count: data.count,\n      total_revenue: Math.round(data.total * 100) / 100,\n    });\n  }\n\n  return summaries.sort((a, b) => a.month - b.month);\n}\n\n/**\n * Generate a report for a specific customer.\n */\nexport function getCustomerReport(\n  db: Database.Database,\n  customerId: string,\n): CustomerReport {\n  const stmt = db.prepare(`\n    SELECT * FROM invoices\n    WHERE customer_id = ?\n  `);\n\n  const invoices = stmt.all(customerId) as Invoice[];\n\n  const totalSpent = invoices.reduce((sum, inv) => sum + inv.total, 0);\n\n  return {\n    customer_id: customerId,\n    invoice_count: invoices.length,\n    total_spent: Math.round(totalSpent * 100) / 100,\n    average_invoice: invoices.length > 0\n      ? Math.round((totalSpent / invoices.length) * 100) / 100\n      : 0,\n  };\n}\n\n/**\n * Get total revenue for a date period.\n */\nexpor