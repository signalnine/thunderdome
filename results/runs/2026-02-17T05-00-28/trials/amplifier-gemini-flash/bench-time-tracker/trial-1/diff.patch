+import { describe, it, expect, beforeEach, afterEach } from 'vitest';
+import { TimeTracker } from './tracker.js';
+import { ValidationError, StateError } from './errors.js';
+import * as fs from 'node:fs';
+import * as path from 'node:path';
+
+const TEST_STORAGE_PATH = './test-data/time-test.json';
+
+describe('TimeTracker', () => {
+  let tracker: TimeTracker;
+
+  beforeEach(() => {
+    tracker = new TimeTracker(TEST_STORAGE_PATH);
+  });
+
+  afterEach(() => {
+    if (fs.existsSync(TEST_STORAGE_PATH)) {
+      fs.unlinkSync(TEST_STORAGE_PATH);
+    }
+    const dir = path.dirname(TEST_STORAGE_PATH);
+    if (fs.existsSync(dir)) {
+      fs.rmdirSync(dir, { recursive: true });
+    }
+  });
+
+  it('should start a named timer', async () => {
+    await tracker.start('Task 1');
+    const entries = await tracker.getEntries();
+    expect(entries).toHaveLength(1);
+    expect(entries[0].taskName).toBe('Task 1');
+    expect(entries[0].endTime).toBeNull();
+  });
+
+  it('should throw error when starting with empty name', async () => {
+    await expect(tracker.start('')).rejects.toThrow(ValidationError);
+    await expect(tracker.start('   ')).rejects.toThrow(ValidationError);
+  });
+
+  it('should stop a running timer', async () => {
+    await tracker.start('Task 1');
+    await tracker.stop();
+    const entries = await tracker.getEntries();
+    expect(entries).toHaveLength(1);
+    expect(entries[0].endTime).not.toBeNull();
+  });
+
+  it('should throw error when stopping and no timer is running', async () => {
+    await expect(tracker.stop()).rejects.toThrow(StateError);
+  });
+
+  it('should automatically stop previous timer when starting a new one', async () => {
+    await tracker.start('Task 1');
+    // Wait a bit to ensure different timestamps if needed, though ISO strings usually differ
+    await new Promise(resolve => setTimeout(resolve, 10));
+    await tracker.start('Task 2');
+    
+    const entries = await tracker.getEntries();
+    expect(entries).toHaveLength(2);
+    expect(entries[0].taskName).toBe('Task 1');
+    expect(entries[0].endTime).not.toBeNull();
+    expect(entries[1].taskName).toBe('Task 2');
+    expect(entries[1].endTime).toBeNull();
+  });
+
+  it('should persist data between instances', async () => {
+    await tracker.start('Persist Task');
+    await tracker.stop();
+
+    const newTracker = new TimeTracker(TEST_STORAGE_PATH);
+    const entries = await newTracker.getEntries();
+    expect(entries).toHaveLength(1);
+    expect(entries[0].taskName).toBe('Persist Task');
+  });
+
+  it('should filter entries by date range', async () => {
+    const now = new Date();
+    const past = new Date(now.getTime() - 10000);
+    const future = new Date(now.getTime() + 10000);
+
+    await tracker.start('Task 1');
+    await tracker.stop();
+
+    const entriesAll = await tracker.getEntries();
+    expect(entriesAll).toHaveLength(1);
+
+    const entriesInRange = await tracker.getEntries({ from: past, to: future });
+    expect(entriesInRange).toHaveLength(1);
+
+    const entriesOutRange = await tracker.getEntries({ from: future });
+    expect(entriesOutRange).toHaveLength(0);
+  });
+
+  it('should summarize time correctly including running tasks', async () => {
+    await tracker.start('Task 1');
+    await new Promise(resolve => setTimeout(resolve, 100));
+    await tracker.stop();
+
+    await tracker.start('Task 1');
+    await new Promise(resolve => setTimeout(resolve, 100));
+    // Task 1 is still running
+
+    const summary = await tracker.getSummary();
+    expect(summary['Task 1']).toBeGreaterThanOrEqual(200);
+  });
+
+  it('should support unicode and emoji in task names', async () => {
+    const unicodeName = 'ğŸš€ Work æ±‰å­—';
+    await tracker.start(unicodeName);
+    await tracker.stop();
+    
+    const entries = await tracker.getEntries();
+    expect(entries[0].taskName).toBe(unicodeName);
+    
+    const summary = await tracker.getSummary();
+    expect(summary[unicodeName]).toBeDefined();
+  });
+});
