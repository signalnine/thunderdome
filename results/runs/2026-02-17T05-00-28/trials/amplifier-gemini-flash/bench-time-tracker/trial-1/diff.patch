@@ -0,0 +1,95 @@
+import { FileStorage } from './storage.js';
+import { ValidationError, StateError } from './errors.js';
+import type { TimeEntry } from './types.js';
+
+export class TimeTracker {
+  private storage: FileStorage;
+
+  constructor(storagePath: string) {
+    this.storage = new FileStorage(storagePath);
+  }
+
+  async start(taskName: string): Promise<void> {
+    if (!taskName || taskName.trim() === '') {
+      throw new ValidationError('Task name cannot be empty');
+    }
+
+    const data = await this.storage.load();
+
+    if (data.activeTaskName !== null) {
+      await this.stop();
+      // Reload data after stop to get updated entries
+      return this.start(taskName);
+    }
+
+    const newEntry: TimeEntry = {
+      taskName,
+      startTime: new Date().toISOString(),
+      endTime: null,
+    };
+
+    data.entries.push(newEntry);
+    data.activeTaskName = taskName;
+
+    await this.storage.save(data);
+  }
+
+  async stop(): Promise<void> {
+    const data = await this.storage.load();
+
+    if (data.activeTaskName === null) {
+      throw new StateError('No timer is currently running');
+    }
+
+    // Find the last entry for the active task that hasn't been stopped
+    let activeEntry: TimeEntry | undefined;
+    for (let i = data.entries.length - 1; i >= 0; i--) {
+      const entry = data.entries[i];
+      if (entry.taskName === data.activeTaskName && entry.endTime === null) {
+        activeEntry = entry;
+        break;
+      }
+    }
+
+    if (activeEntry) {
+      activeEntry.endTime = new Date().toISOString();
+    }
+
+    data.activeTaskName = null;
+    await this.storage.save(data);
+  }
+
+  async getEntries(filter?: { from?: Date; to?: Date }): Promise<TimeEntry[]> {
+    const data = await this.storage.load();
+    let entries = data.entries;
+
+    if (filter) {
+      if (filter.from) {
+        const fromTime = filter.from.getTime();
+        entries = entries.filter((e) => new Date(e.startTime).getTime() >= fromTime);
+      }
+      if (filter.to) {
+        const toTime = filter.to.getTime();
+        entries = entries.filter((e) => new Date(e.startTime).getTime() <= toTime);
+      }
+    }
+
+    return entries;
+  }
+
+  async getSummary(): Promise<Record<string, number>> {
+    const data = await this.storage.load();
+    const summary: Record<string, number> = {};
+    const now = Date.now();
+
+    for (const entry of data.entries) {
+      const start = new Date(entry.startTime).getTime();
+      const end = entry.endTime ? new Date(entry.endTime).getTime() : now;
+      const duration = end - start;
+
+      summary[entry.taskName] = (summary[entry.taskName] || 0) + duration;
+    }
+
+    return summary;
+  }
+}
