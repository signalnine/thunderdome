+import { readFileSync, writeFileSync, existsSync, mkdirSync } from 'fs';
+import { dirname } from 'path';
+
+export interface Entry {
+  taskName: string;
+  startTime: string;
+  endTime: string | null;
+}
+
+export class TimeTracker {
+  private filePath: string;
+  private entries: Entry[] = [];
+
+  constructor(filePath: string) {
+    this.filePath = filePath;
+    this.loadData();
+  }
+
+  private loadData(): void {
+    if (existsSync(this.filePath)) {
+      try {
+        const data = readFileSync(this.filePath, 'utf-8');
+        this.entries = JSON.parse(data);
+      } catch (error) {
+        this.entries = [];
+      }
+    }
+  }
+
+  private saveData(): void {
+    const dir = dirname(this.filePath);
+    if (!existsSync(dir)) {
+      mkdirSync(dir, { recursive: true });
+    }
+    writeFileSync(this.filePath, JSON.stringify(this.entries, null, 2), 'utf-8');
+  }
+
+  start(taskName: string): Entry {
+    if (!taskName || taskName.trim() === '') {
+      throw new Error('Task name cannot be empty');
+    }
+
+    // Auto-stop any running task
+    const activeTask = this.getActiveTask();
+    if (activeTask) {
+      activeTask.endTime = new Date().toISOString();
+    }
+
+    // Create new entry
+    const entry: Entry = {
+      taskName,
+      startTime: new Date().toISOString(),
+      endTime: null
+    };
+
+    this.entries.push(entry);
+    this.saveData();
+
+    return entry;
+  }
+
+  stop(): Entry {
+    const activeTask = this.getActiveTask();
+    if (!activeTask) {
+      throw new Error('No active task to stop');
+    }
+
+    activeTask.endTime = new Date().toISOString();
+    this.saveData();
+
+    return activeTask;
+  }
+
+  getActiveTask(): Entry | null {
+    return this.entries.find(entry => entry.endTime === null) || null;
+  }
+
+  getLog(options?: { since?: Date }): Entry[] {
+    if (!options?.since) {
+      return this.entries;
+    }
+
+    const sinceTime = options.since.getTime();
+
+    return this.entries.filter(entry => {
+      const startTime = new Date(entry.startTime).getTime();
+
+      // Include if started on or after since date
+      if (startTime >= sinceTime) {
+        return true;
+      }
+
+      // Include if still running (no endTime)
+      if (entry.endTime === null) {
+        return true;
+      }
+
+      // Include if endTime is on or after since date
+      const endTime = new Date(entry.endTime).getTime();
+      if (endTime >= sinceTime) {
+        return true;
+      }
+
+      return false;
+    });
+  }
+
+  getSummary(): Record<string, number> {
+    const summary: Record<string, number> = {};
+
+    for (const entry of this.entries) {
+      const startTime = new Date(entry.startTime).getTime();
+      const endTime = entry.endTime
+        ? new Date(entry.endTime).getTime()
+        : Date.now();
+
+      const duration = endTime - startTime;
+
+      if (summary[entry.taskName]) {
+        summary[entry.taskName] += duration;
+      } else {
+        summary[entry.taskName] = duration;
+      }
+    }
+
+    return summary;
+  }
+}
