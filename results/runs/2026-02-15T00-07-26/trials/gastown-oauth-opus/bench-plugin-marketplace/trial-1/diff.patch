+import { describe, it, expect, beforeEach } from 'vitest';
+import { createDepsService } from '../src/deps.js';
+import { createRegistry } from '../src/registry.js';
+import type { RegistryService, DepsService, PluginManifest } from '../src/types.js';
+
+function makeManifest(
+  name: string,
+  deps: Record<string, string> = {}
+): PluginManifest {
+  return {
+    name,
+    version: '1.0.0',
+    description: `Plugin ${name}`,
+    author: 'tester',
+    dependencies: deps,
+    entryPoint: '1',
+    permissions: [],
+  };
+}
+
+describe('Dependency Graph', () => {
+  let depsService: DepsService;
+  let registry: RegistryService;
+
+  beforeEach(() => {
+    depsService = createDepsService();
+    registry = createRegistry();
+  });
+
+  it('should build a dependency tree', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0', c: '^1.0.0' }));
+    registry.register(makeManifest('b', { d: '^1.0.0' }));
+    registry.register(makeManifest('c'));
+    registry.register(makeManifest('d'));
+
+    const tree = depsService.buildTree('a', registry);
+    expect(tree.name).toBe('a');
+    expect(tree.dependencies).toHaveLength(2);
+    expect(tree.dependencies[0].name).toBe('b');
+    expect(tree.dependencies[0].dependencies).toHaveLength(1);
+    expect(tree.dependencies[0].dependencies[0].name).toBe('d');
+    expect(tree.dependencies[1].name).toBe('c');
+  });
+
+  it('should handle missing dependencies gracefully', () => {
+    registry.register(makeManifest('a', { missing: '^1.0.0' }));
+    const tree = depsService.buildTree('a', registry);
+    expect(tree.dependencies).toHaveLength(1);
+    expect(tree.dependencies[0].name).toBe('missing');
+    expect(tree.dependencies[0].version).toBe('unknown');
+  });
+
+  it('should detect circular dependencies', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0' }));
+    registry.register(makeManifest('b', { a: '^1.0.0' }));
+
+    const cycles = depsService.detectCycles('a', registry);
+    expect(cycles).not.toBeNull();
+    expect(cycles!.length).toBeGreaterThan(0);
+  });
+
+  it('should return null when no cycles exist', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0' }));
+    registry.register(makeManifest('b'));
+
+    const cycles = depsService.detectCycles('a', registry);
+    expect(cycles).toBeNull();
+  });
+
+  it('should detect hasCircularDependency', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0' }));
+    registry.register(makeManifest('b', { a: '^1.0.0' }));
+
+    expect(depsService.hasCircularDependency('a', registry)).toBe(true);
+  });
+
+  it('should report no circular dependency for acyclic graph', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0' }));
+    registry.register(makeManifest('b'));
+
+    expect(depsService.hasCircularDependency('a', registry)).toBe(false);
+  });
+
+  it('should produce topological ordering', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0', c: '^1.0.0' }));
+    registry.register(makeManifest('b', { d: '^1.0.0' }));
+    registry.register(makeManifest('c'));
+    registry.register(makeManifest('d'));
+
+    const sorted = depsService.topologicalSort('a', registry);
+    expect(sorted).toContain('a');
+    expect(sorted).toContain('b');
+    expect(sorted).toContain('c');
+    expect(sorted).toContain('d');
+
+    // d must come before b, b and c must come before a
+    expect(sorted.indexOf('d')).toBeLessThan(sorted.indexOf('b'));
+    expect(sorted.indexOf('b')).toBeLessThan(sorted.indexOf('a'));
+    expect(sorted.indexOf('c')).toBeLessThan(sorted.indexOf('a'));
+  });
+
+  it('should flatten transitive dependencies', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0', c: '^1.0.0' }));
+    registry.register(makeManifest('b', { d: '^1.0.0' }));
+    registry.register(makeManifest('c'));
+    registry.register(makeManifest('d'));
+
+    const flattened = depsService.flattenDependencies('a', registry);
+    expect(flattened).toContain('b');
+    expect(flattened).toContain('c');
+    expect(flattened).toContain('d');
+    expect(flattened).not.toContain('a');
+  });
+
+  it('should handle plugin with no dependencies', () => {
+    registry.register(makeManifest('solo'));
+
+    const tree = depsService.buildTree('solo', registry);
+    expect(tree.dependencies).toHaveLength(0);
+
+    const sorted = depsService.topologicalSort('solo', registry);
+    expect(sorted).toEqual(['solo']);
+
+    const flattened = depsService.flattenDependencies('solo', registry);
+    expect(flattened).toEqual([]);
+  });
+
+  it('should detect three-node cycle', () => {
+    registry.register(makeManifest('a', { b: '^1.0.0' }));
+    registry.register(makeManifest('b', { c: '^1.0.0' }));
+    registry.register(makeManifest('c', { a: '^1.0.0' }));
+
+    expect(depsService.hasCircularDependency('a', registry)).toBe(true);
+    const cycles = depsService.detectCycles('a', registry);
+    expect(cycles).not.toBeNull();
+  });
+});
