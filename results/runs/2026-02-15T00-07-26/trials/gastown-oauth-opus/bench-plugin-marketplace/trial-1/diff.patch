+import { describe, it, expect, beforeEach } from 'vitest';
+import { createReviewService } from '../src/reviews.js';
+import type { ReviewService } from '../src/types.js';
+
+describe('Review System', () => {
+  let reviews: ReviewService;
+
+  beforeEach(() => {
+    reviews = createReviewService();
+  });
+
+  it('should add a review', () => {
+    const review = reviews.addReview({
+      pluginName: 'test-plugin',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great plugin!',
+    });
+    expect(review.id).toBe(1);
+    expect(review.pluginName).toBe('test-plugin');
+    expect(review.rating).toBe(5);
+    expect(review.flagged).toBe(false);
+    expect(review.createdAt).toBeDefined();
+  });
+
+  it('should reject invalid ratings', () => {
+    expect(() =>
+      reviews.addReview({
+        pluginName: 'test',
+        pluginVersion: '1.0.0',
+        userId: 'user1',
+        rating: 0,
+        comment: 'Bad',
+      })
+    ).toThrow('Rating must be between 1 and 5');
+
+    expect(() =>
+      reviews.addReview({
+        pluginName: 'test',
+        pluginVersion: '1.0.0',
+        userId: 'user1',
+        rating: 6,
+        comment: 'Too high',
+      })
+    ).toThrow('Rating must be between 1 and 5');
+  });
+
+  it('should get reviews for a plugin', () => {
+    reviews.addReview({
+      pluginName: 'plugin-a',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great!',
+    });
+    reviews.addReview({
+      pluginName: 'plugin-b',
+      pluginVersion: '1.0.0',
+      userId: 'user2',
+      rating: 3,
+      comment: 'OK',
+    });
+    reviews.addReview({
+      pluginName: 'plugin-a',
+      pluginVersion: '1.0.0',
+      userId: 'user2',
+      rating: 4,
+      comment: 'Good',
+    });
+
+    const pluginAReviews = reviews.getReviews('plugin-a');
+    expect(pluginAReviews).toHaveLength(2);
+  });
+
+  it('should compute average rating excluding flagged reviews', () => {
+    reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great',
+    });
+    const r2 = reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user2',
+      rating: 1,
+      comment: 'Spam',
+    });
+    reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user3',
+      rating: 3,
+      comment: 'OK',
+    });
+
+    // Before flagging: average is (5 + 1 + 3) / 3 = 3
+    expect(reviews.getAverageRating('test')).toBe(3);
+
+    // Flag the spam review
+    reviews.flagReview(r2.id);
+
+    // After flagging: average is (5 + 3) / 2 = 4
+    expect(reviews.getAverageRating('test')).toBe(4);
+  });
+
+  it('should return 0 average for no reviews', () => {
+    expect(reviews.getAverageRating('nonexistent')).toBe(0);
+  });
+
+  it('should flag reviews', () => {
+    const review = reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 1,
+      comment: 'Spam',
+    });
+    reviews.flagReview(review.id);
+    const flagged = reviews.getFlaggedReviews();
+    expect(flagged).toHaveLength(1);
+    expect(flagged[0].id).toBe(review.id);
+    expect(flagged[0].flagged).toBe(true);
+  });
+
+  it('should throw when flagging non-existent review', () => {
+    expect(() => reviews.flagReview(999)).toThrow('Review 999 not found');
+  });
+
+  it('should remove a review', () => {
+    const review = reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great',
+    });
+    expect(reviews.removeReview(review.id)).toBe(true);
+    expect(reviews.getReviews('test')).toHaveLength(0);
+  });
+
+  it('should return false when removing non-existent review', () => {
+    expect(reviews.removeReview(999)).toBe(false);
+  });
+
+  it('should get reviews by user', () => {
+    reviews.addReview({
+      pluginName: 'a',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great',
+    });
+    reviews.addReview({
+      pluginName: 'b',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 4,
+      comment: 'Good',
+    });
+    reviews.addReview({
+      pluginName: 'a',
+      pluginVersion: '1.0.0',
+      userId: 'user2',
+      rating: 3,
+      comment: 'OK',
+    });
+
+    const user1Reviews = reviews.getReviewsByUser('user1');
+    expect(user1Reviews).toHaveLength(2);
+  });
+
+  it('should get review count', () => {
+    reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user1',
+      rating: 5,
+      comment: 'Great',
+    });
+    reviews.addReview({
+      pluginName: 'test',
+      pluginVersion: '1.0.0',
+      userId: 'user2',
+      rating: 4,
+      comment: 'Good',
+    });
+    expect(reviews.getReviewCount('test')).toBe(2);
+    expect(reviews.getReviewCount('other')).toBe(0);
+  });
+});
