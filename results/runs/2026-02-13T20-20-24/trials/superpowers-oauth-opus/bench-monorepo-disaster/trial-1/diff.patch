r = getUserById(userId);\n    62→  if (!user) {\n    63→    console.error(`User ${userId} not found for sync`);\n    64→    return false;\n    65→  }\n    66→\n    67→  // In a real app, we'd sync to an external service here\n    68→  console.log(`Synced user ${user.name} (${user.email})`);\n    69→  return true;\n    70→}\n    71→"}]},"parent_tool_use_id":null,"session_id":"fdc0c45a-f96e-4996-9c9f-942829109746","uuid":"dc6e856f-84b4-4d46-b2cc-32ccd0fd6786","tool_use_result":{"type":"text","file":{"filePath":"/workspace/packages/worker/src/jobs/sync.ts","content":"/**\n * Data sync job.\n * Syncs external user data into the local database.\n */\n\nimport {\n  createUser,\n  getUserById,\n  getAllUsers,\n  type CreateUserInput,\n} from '@monorepo/core';\n\ninterface ExternalUser {\n  name: string;\n  email: string;\n  role: 'admin' | 'user' | 'viewer';\n}\n\n// Simulated external data source\nfunction fetchExternalUsers(): ExternalUser[] {\n  return [\n    { name: 'External User 1', email: 'ext1@example.com', role: 'user' },\n    { name: 'External User 2', email: 'ext2@example.com', role: 'viewer' },\n  ];\n}\n\nexport async function syncJob(): Promise<void> {\n  const externalUsers = fetchExternalUsers();\n  const existingUsers = getAllUsers();\n  const existingEmails = new Set(existingUsers.map((u) => u.email));\n\n  let synced = 0;\n  let skipped = 0;\n\n  for (const extUser of externalUsers) {\n    if (existingEmails.has(extUser.email)) {\n      skipped++;\n      continue;\n    }\n\n    const input: CreateUserInput = {\n      name: extUser.name,\n      email: extUser.email,\n      role: extUser.role,\n    };\n\n    try {\n      const newUser = createUser(input);\n      if (newUser.id) {\n        synced++;\n      }\n    } catch (err) {\n      console.error(`Failed to sync user ${extUser.email}:`, err);\n    }\n  }\n\n  console.log(`Sync complete: ${synced} synced, ${skipped} skipped`);\n}\n\nexport async function syncSingleUser(userId: number): Promise<boolean> {\