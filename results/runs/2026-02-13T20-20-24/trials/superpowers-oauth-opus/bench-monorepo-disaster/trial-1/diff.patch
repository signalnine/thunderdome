re';\n\nexport const usersRouter = Router();\n\n// List all users\nusersRouter.get('/', (_req: Request, res: Response) => {\n  try {\n    const users = getAllUsers();\n    res.json({ users });\n  } catch (err) {\n    res.status(500).json({ error: 'Failed to fetch users' });\n  }\n});\n\n// Get user by ID\nusersRouter.get('/:id', (req: Request, res: Response) => {\n  try {\n    const validation = validateUserId(req.params['id']!);\n    if (!validation.success) {\n      res.status(400).json({ errors: validation.errors });\n      return;\n    }\n\n    const user = getUserById(validation.data!);\n    if (!user) {\n      res.status(404).json({ error: 'User not found' });\n      return;\n    }\n    res.json({ user });\n  } catch (err) {\n    res.status(500).json({ error: 'Failed to fetch user' });\n  }\n});\n\n// Create a new user\nusersRouter.post('/', (req: Request, res: Response) => {\n  try {\n    const callerRole = (req.headers['x-user-role'] as Role) || 'viewer';\n    if (!canManageUsers(callerRole)) {\n      res.status(403).json({ error: 'Insufficient permissions' });\n      return;\n    }\n\n    const validation = validateCreateUser(req.body);\n    if (!validation.success) {\n      res.status(400).json({ errors: validation.errors });\n      return;\n    }\n\n    const user = createUser(req.body);\n    res.status(201).json({ user });\n  } catch (err) {\n    if (err instanceof Error && err.message.includes('UNIQUE')) {\n      res.status(409).json({ error: 'Email already exists' });\n      return;\n    }\n    res.status(500).json({ error: 'Failed to create user' });\n  }\n});\n\n// Delete user by ID\nusersRouter.delete('/:id', (req: Request, res: Response) => {\n  try {\n    const callerRole = (req.headers['x-user-role'] as Role) || 'viewer';\n    if (!canManageUsers(callerRole)) {\n      res.status(403).json({ error: 'Insufficient permissions' });\n      return;\n    }\n\n    const validation = validateUserId(req.params['id']!);\n    if (!validation.success) {\n      res.status(400).json({ errors: validation.errors });\n      return;\n    }\n\n    const user = getUserById(validation.data!);\n    if (!user) {\n      res.status(404).json({ error: 'User not found' });\n      return;\n    }\n\n    // In a real app, we'd delete here\n    res.status(200).json({ message: 'User deleted', user });\n  } catch (err) {\n    res.status(500).json({ error: 'Failed to delete user' });\n  }\n});\n","numLines":101,"startLine":1,"totalLines":101}}}
