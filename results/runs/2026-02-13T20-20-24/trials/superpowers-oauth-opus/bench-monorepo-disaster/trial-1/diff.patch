;\n}\n\n// Start server if run directly\nconst isMainModule = process.argv[1]?.includes('api/dist/index.js');\nif (isMainModule) {\n  const port = process.env['PORT'] || 3000;\n  const app = createApp();\n  app.listen(port, () => {\n    console.log(`API server running on port ${port}`);\n  });\n}\n```\n\n**`/workspace/packages/api/src/middleware/auth.ts`** - Authentication middleware\n```typescript\n/**\n * Authentication middleware using core permissions.\n */\n\nimport type { Request, Response, NextFunction } from 'express';\nimport { hasPermission, type Role, type Permission } from '@monorepo/core';\n\n/**\n * Basic auth middleware that checks for a token header.\n * In this benchmark, we use a simplified mock auth system.\n */\nexport function authMiddleware(\n  req: Request,\n  res: Response,\n  next: NextFunction\n): void {\n  const token = req.headers['authorization']?.replace('Bearer ', '');\n\n  // Allow requests with valid mock tokens or role headers\n  if (token?.startsWith('mock-token-') || req.headers['x-user-role']) {\n    next();\n    return;\n  }\n\n  res.status(401).json({ error: 'Authentication required' });\n}\n\n/**\n * Middleware factory to require a specific permission.\n */\nexport function requirePermissionMiddleware(permission: Permission) {\n  return (req: Request, res: Response, next: NextFunction): void => {\n    const role = (req.headers['x-user-role'] as Role) || 'viewer';\n\n    if (!hasPermission(role, permission)) {\n      res.status(403).json({\n        error: `Permission '${permission}' required`,\n      });\n      return;\n    }\n\n    next();\n  };\n}\n```\n\n**`/workspace/packages/api/src/routes/auth.ts`** - Auth routes\n```typescript\n/**\n * Authentication routes.\n */\n\nimport { Router, type Request, type Response } from 'express';\nimport { validateCreateUser, registerUser, type Role } from '@monorepo/core';\n\nexport const authRouter = Router();\n\n// Simple registration (public)\nauthRouter.post('/register', (req: Request