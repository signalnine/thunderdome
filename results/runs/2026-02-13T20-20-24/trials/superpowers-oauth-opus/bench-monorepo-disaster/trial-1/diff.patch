is.jobs) {\n      if (job.timer) {\n        clearInterval(job.timer);\n        job.timer = undefined;\n      }\n    }\n  }\n\n  async runOnce(name: string): Promise<void> {\n    const job = this.jobs.get(name);\n    if (!job) {\n      throw new Error(`Job '${name}' not found`);\n    }\n\n    await job.handler();\n    job.lastRun = new Date();\n    job.runCount++;\n  }\n\n  getStatus(): Array<{\n    name: string;\n    lastRun: Date | undefined;\n    runCount: number;\n    errorCount: number;\n  }> {\n    return Array.from(this.jobs.values()).map((job) => ({\n      name: job.name,\n      lastRun: job.lastRun,\n      runCount: job.runCount,\n      errorCount: job.errors.length,\n    }));\n  }\n\n  isRunning(): boolean {\n    return this.running;\n  }\n}\n```\n\n**`/workspace/packages/worker/src/jobs/sync.ts`** - Data sync job\n```typescript\n/**\n * Data sync job.\n * Syncs external user data into the local database.\n */\n\nimport {\n  createUser,\n  getUserById,\n  getAllUsers,\n  type CreateUserInput,\n} from '@monorepo/core';\n\ninterface ExternalUser {\n  name: string;\n  email: string;\n  role: 'admin' | 'user' | 'viewer';\n}\n\n// Simulated external data source\nfunction fetchExternalUsers(): ExternalUser[] {\n  return [\n    { name: 'External User 1', email: 'ext1@example.com', role: 'user' },\n    { name: 'External User 2', email: 'ext2@example.com', role: 'viewer' },\n  ];\n}\n\nexport async function syncJob(): Promise<void> {\n  const externalUsers = fetchExternalUsers();\n  const existingUsers = getAllUsers();\n  const existingEmails = new Set(existingUsers.map((u) => u.email));\n\n  let synced = 0;\n  let skipped = 0;\n\n  for (const extUser of externalUsers) {\n    if (existingEmails.has(extUser.email)) {\n      skipped++;\n      continue;\n    }\n\n    const input: CreateUserInput = {\n      name: extUser.name,\n      email: extUser.email,\n      role: extUser.role,\n    };\n\n    try {\n      const newUser = createUser(input);\n      if (newUser.id) {\n