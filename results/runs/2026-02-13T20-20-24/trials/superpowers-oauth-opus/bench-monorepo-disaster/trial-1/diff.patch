+{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01EQxwUwVYm3iT9wBbjf1SCg","type":"tool_result","content":"     1→/**\n     2→ * Database connection factory and table creation.\n     3→ * Uses better-sqlite3 for a simple embedded database.\n     4→ */\n     5→\n     6→import Database from 'better-sqlite3';\n     7→import type { User } from './types.js';\n     8→\n     9→export interface DatabaseConnection {\n    10→  db: Database.Database;\n    11→  close: () => void;\n    12→}\n    13→\n    14→export function createDatabase(path: string = ':memory:'): DatabaseConnection {\n    15→  const db = new Database(path);\n    16→\n    17→  // Enable WAL mode for better concurrent read performance\n    18→  db.pragma('journal_mode = WAL');\n    19→  db.pragma('foreign_keys = ON');\n    20→\n    21→  return {\n    22→    db,\n    23→    close: () => db.close(),\n    24→  };\n    25→}\n    26→\n    27→export function createTables(conn: DatabaseConnection): void {\n    28→  conn.db.exec(`\n    29→    CREATE TABLE IF NOT EXISTS users (\n    30→      id INTEGER PRIMARY KEY AUTOINCREMENT,\n    31→      name TEXT NOT NULL,\n    32→      email TEXT NOT NULL UNIQUE,\n    33→      role TEXT NOT NULL CHECK(role IN ('admin', 'user', 'viewer')),\n    34→      created_at TEXT NOT NULL DEFAULT (datetime('now')),\n    35→      updated_at TEXT NOT NULL DEFAULT (datetime('now'))\n    36→    );\n    37→\n    38→    CREATE INDEX IF NOT EXISTS idx_users_email ON users(email);\n    39→    CREATE INDEX IF NOT EXISTS idx_users_role ON users(role);\n    40→  `);\n    41→}\n    42→\n    43→export function insertUser(\n    44→  conn: DatabaseConnection,\n    45→  user: { name: string; email: string; role: string }\n    46→): User {\n    47→  const stmt = conn.db.prepare(\n    48→    'INSERT INTO users (name, email, role) VALUES (?, ?, ?) RETURNING *'\n    49→  );\n    50→  const row = stmt.get(user.name, user.email, user.role) as {\n    51→    id: number;\n    52→    name: string;\n    53→    email: string;\n    54→    role: string;\n    55→    created_at: string;\n    56→    updated_at: string;\n    57→  };\n    58→  return {\n    59→    id: row.id,\n    60→    name: row.name,\n    61→    email: row.email,\n    62→    role: row.role as User['role'],\n    63→    createdAt: row.created_at,\n    64→    updatedAt: row.updated_at,\n    65→  };\n    66→}\n    67→\n    68→export function findUserById(\n    69→  conn: DatabaseConnection,\n    70→  id: number\n    71→): User | null {\n    72→  const stmt = conn.db.prepare('SELECT * FROM users WHERE id = ?');\n    73→  const row = stmt.get(id) as {\n    74→    id: number;\n    75→    name: string;\n    76→    email: string;\n    77→    role: string;\n    78→    created_at: string;\n    79→    updated_at: string;\n    80→  } | undefined;\n    81→  if (!row) return null;\n    82→  return {\n    83→    id: row.id,\n    84→    name: row.name,\n    85→    email: row.email,\n    86→    role: row.role as User['role'],\n    87→    createdAt: row.created_at,\n    88→    updatedAt: row.updated_at,\n    89→  };\n    90→}\n    91→\n    92→export function findAllUsers(conn: DatabaseConnection): User[] {\n    93→  const stmt = conn.db.prepare('SELECT * FROM users ORDER BY id');\n    94→  const rows = stmt.all() as Array<{\n    95→    id: number;\n    96→    name: string;\n    97→    email: string;\n    98→    role: string;\n    99→    created_at: string;\n   100→    updated_at: string;\n   101→  }>;\n   102→  return rows.map((row) => ({\n   103→    id: row.id,\n   104→    name: row.name,\n   105→    email: row.email,\n   106→    role: row.role as User['role'],\n   107→    createdAt: row.created_at,\n   108→    updatedAt: row.updated_at,\n   109→  }));\n   110→}\n   111→"}]},"parent_tool_use_id":"toolu_01DKyEimKvL8FiEr7t4VugZ5","session_id":"fdc0c45a-f96e-4996-9c9f-942829109746","uuid":"29d40829-03be-4034-9928-5a257e6082bf"}
