+import { Router } from 'express';
+import type { Request, Response } from 'express';
+import type { DB } from '../db.js';
+import { getAuthUser } from '../auth.js';
+
+type ParamValue = string | number;
+
+function buildFilterClauses(
+  filters: Record<string, unknown> | undefined,
+  params: ParamValue[]
+): string {
+  if (!filters || typeof filters !== 'object') return '';
+  let clause = '';
+  for (const [key, value] of Object.entries(filters)) {
+    clause += ' AND json_extract(properties, ?) = ?';
+    params.push(`$.${key}`);
+    params.push(String(value));
+  }
+  return clause;
+}
+
+export function createQueryRoutes(db: DB): Router {
+  const router = Router();
+
+  // POST /count
+  router.post('/count', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const { event_name, start_date, end_date, filters } = req.body ?? {};
+    let query = 'SELECT COUNT(*) as count FROM events WHERE tenant_id = ?';
+    const params: ParamValue[] = [user.tenant_id];
+
+    if (event_name) {
+      query += ' AND name = ?';
+      params.push(event_name);
+    }
+
+    if (start_date) {
+      query += ' AND timestamp >= ?';
+      params.push(start_date);
+    }
+
+    if (end_date) {
+      query += ' AND timestamp <= ?';
+      params.push(end_date);
+    }
+
+    query += buildFilterClauses(filters, params);
+
+    const result = db.prepare(query).get(...params) as { count: number };
+    res.json({ count: result.count });
+  });
+
+  // POST /timeseries
+  router.post('/timeseries', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const { event_name, start_date, end_date, interval, filters } = req.body ?? {};
+
+    let bucketExpr: string;
+    switch (interval) {
+      case 'hour':
+        bucketExpr = "strftime('%Y-%m-%d %H:00:00', timestamp)";
+        break;
+      case 'week':
+        bucketExpr = "strftime('%Y-%W', timestamp)";
+        break;
+      case 'month':
+        bucketExpr = "strftime('%Y-%m', timestamp)";
+        break;
+      default:
+        bucketExpr = 'date(timestamp)';
+        break;
+    }
+
+    let query = `SELECT ${bucketExpr} as bucket, COUNT(*) as count FROM events WHERE tenant_id = ?`;
+    const params: ParamValue[] = [user.tenant_id];
+
+    if (event_name) {
+      query += ' AND name = ?';
+      params.push(event_name);
+    }
+
+    if (start_date) {
+      query += ' AND timestamp >= ?';
+      params.push(start_date);
+    }
+
+    if (end_date) {
+      query += ' AND timestamp <= ?';
+      params.push(end_date);
+    }
+
+    query += buildFilterClauses(filters, params);
+    query += ' GROUP BY bucket ORDER BY bucket';
+
+    const results = db.prepare(query).all(...params) as Array<{ bucket: string; count: number }>;
+    res.json({ data: results });
+  });
+
+  // POST /breakdown
+  router.post('/breakdown', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const { event_name, property, start_date, end_date, filters, limit: resultLimit } =
+      req.body ?? {};
+
+    if (!property) {
+      res.status(400).json({ error: 'property is required for breakdown query' });
+      return;
+    }
+
+    let query =
+      'SELECT json_extract(properties, ?) as value, COUNT(*) as count FROM events WHERE tenant_id = ?';
+    const params: ParamValue[] = [`$.${property}`, user.tenant_id];
+
+    if (event_name) {
+      query += ' AND name = ?';
+      params.push(event_name);
+    }
+
+    if (start_date) {
+      query += ' AND timestamp >= ?';
+      params.push(start_date);
+    }
+
+    if (end_date) {
+      query += ' AND timestamp <= ?';
+      params.push(end_date);
+    }
+
+    query += buildFilterClauses(filters, params);
+    query += ' GROUP BY value ORDER BY count DESC';
+
+    if (resultLimit) {
+      query += ' LIMIT ?';
+      params.push(parseInt(String(resultLimit), 10));
+    }
+
+    const results = db.prepare(query).all(...params) as Array<{
+      value: string | null;
+      count: number;
+    }>;
+    res.json({ data: results });
+  });
+
+  return router;
+}
