+import { Router } from 'express';
+import type { Request, Response } from 'express';
+import { v4 as uuidv4 } from 'uuid';
+import type { DB } from '../db.js';
+import { getAuthUser } from '../auth.js';
+
+interface DashboardRow {
+  id: string;
+  tenant_id: string;
+  name: string;
+  owner_id: string;
+  config: string;
+  created_at: string;
+  updated_at: string;
+}
+
+interface WidgetRow {
+  id: string;
+  dashboard_id: string;
+  type: string;
+  config: string;
+  position_x: number;
+  position_y: number;
+  width: number;
+  height: number;
+}
+
+export function createDashboardRoutes(db: DB): Router {
+  const router = Router();
+
+  // GET / - List dashboards for tenant
+  router.get('/', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const dashboards = db
+      .prepare('SELECT * FROM dashboards WHERE tenant_id = ? ORDER BY created_at DESC')
+      .all(user.tenant_id) as DashboardRow[];
+
+    res.json({
+      dashboards: dashboards.map((d) => ({
+        ...d,
+        config: JSON.parse(d.config),
+      })),
+    });
+  });
+
+  // POST / - Create dashboard
+  router.post('/', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const { name, config } = req.body ?? {};
+    if (!name) {
+      res.status(400).json({ error: 'Dashboard name is required' });
+      return;
+    }
+
+    const id = uuidv4();
+    const configStr = JSON.stringify(config || {});
+
+    db.prepare(
+      'INSERT INTO dashboards (id, tenant_id, name, owner_id, config) VALUES (?, ?, ?, ?, ?)'
+    ).run(id, user.tenant_id, name, user.id, configStr);
+
+    res.status(201).json({
+      id,
+      tenant_id: user.tenant_id,
+      name,
+      owner_id: user.id,
+      config: config || {},
+    });
+  });
+
+  // GET /:id - Get single dashboard
+  router.get('/:id', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const dashboard = db
+      .prepare('SELECT * FROM dashboards WHERE id = ? AND tenant_id = ?')
+      .get(req.params.id, user.tenant_id) as DashboardRow | undefined;
+
+    if (!dashboard) {
+      res.status(404).json({ error: 'Dashboard not found' });
+      return;
+    }
+
+    const widgets = db
+      .prepare('SELECT * FROM widgets WHERE dashboard_id = ?')
+      .all(dashboard.id) as WidgetRow[];
+
+    res.json({
+      ...dashboard,
+      config: JSON.parse(dashboard.config),
+      widgets: widgets.map((w) => ({ ...w, config: JSON.parse(w.config) })),
+    });
+  });
+
+  // PUT /:id - Update dashboard
+  router.put('/:id', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const dashboard = db
+      .prepare('SELECT * FROM dashboards WHERE id = ? AND tenant_id = ?')
+      .get(req.params.id, user.tenant_id) as DashboardRow | undefined;
+
+    if (!dashboard) {
+      res.status(404).json({ error: 'Dashboard not found' });
+      return;
+    }
+
+    const { name, config } = req.body ?? {};
+    const updatedName = name || dashboard.name;
+    const updatedConfig = config !== undefined ? JSON.stringify(config) : dashboard.config;
+
+    db.prepare("UPDATE dashboards SET name = ?, config = ?, updated_at = datetime('now') WHERE id = ?").run(
+      updatedName,
+      updatedConfig,
+      req.params.id
+    );
+
+    res.json({
+      id: dashboard.id,
+      tenant_id: dashboard.tenant_id,
+      name: updatedName,
+      owner_id: dashboard.owner_id,
+      config: config !== undefined ? config : JSON.parse(dashboard.config),
+    });
+  });
+
+  // DELETE /:id - Delete dashboard
+  router.delete('/:id', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const dashboard = db
+      .prepare('SELECT * FROM dashboards WHERE id = ? AND tenant_id = ?')
+      .get(req.params.id, user.tenant_id) as DashboardRow | undefined;
+
+    if (!dashboard) {
+      res.status(404).json({ error: 'Dashboard not found' });
+      return;
+    }
+
+    db.prepare('DELETE FROM dashboards WHERE id = ?').run(req.params.id);
+
+    res.json({ message: 'Dashboard deleted' });
+  });
+
+  return router;
+}
