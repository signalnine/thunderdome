+++ b/src/app.ts
@@ -0,0 +1,32 @@
+import express from 'express';
+import { createDatabase } from './db.js';
+import { createAuthMiddleware } from './auth.js';
+import { createAuthRoutes, createUserRoutes } from './routes/users.js';
+import { createEventRoutes } from './routes/events.js';
+import { createQueryRoutes } from './routes/queries.js';
+import { createDashboardRoutes } from './routes/dashboards.js';
+import { createExportRoutes } from './routes/export.js';
+
+export interface AppOptions {
+  dbPath?: string;
+}
+
+export function createApp(options?: AppOptions): express.Application {
+  const db = createDatabase(options?.dbPath);
+  const app = express();
+  const auth = createAuthMiddleware(db);
+
+  app.use(express.json({ limit: '10mb' }));
+
+  // Auth routes (register/login are public, profile is protected)
+  app.use('/api/auth', createAuthRoutes(db, auth));
+
+  // Protected routes
+  app.use('/api/users', auth, createUserRoutes(db));
+  app.use('/api/events', auth, createEventRoutes(db));
+  app.use('/api/queries', auth, createQueryRoutes(db));
+  app.use('/api/dashboards', auth, createDashboardRoutes(db));
+  app.use('/api/export', auth, createExportRoutes(db));
+
+  return app;
+}
