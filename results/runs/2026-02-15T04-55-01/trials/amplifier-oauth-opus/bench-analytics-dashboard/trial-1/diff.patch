+import { Router } from 'express';
+import type { Request, Response } from 'express';
+import type { DB } from '../db.js';
+import { getAuthUser } from '../auth.js';
+
+interface EventRow {
+  id: string;
+  tenant_id: string;
+  name: string;
+  properties: string;
+  timestamp: string;
+  user_id: string | null;
+  session_id: string | null;
+}
+
+interface DashboardRow {
+  id: string;
+  tenant_id: string;
+  name: string;
+  owner_id: string;
+  config: string;
+  created_at: string;
+  updated_at: string;
+}
+
+interface WidgetRow {
+  id: string;
+  dashboard_id: string;
+  type: string;
+  config: string;
+  position_x: number;
+  position_y: number;
+  width: number;
+  height: number;
+}
+
+function escapeCsvField(field: string): string {
+  if (field.includes(',') || field.includes('"') || field.includes('\n')) {
+    return `"${field.replace(/"/g, '""')}"`;
+  }
+  return field;
+}
+
+function toCsv(rows: Record<string, unknown>[]): string {
+  if (rows.length === 0) return '';
+  const headers = Object.keys(rows[0]);
+  const lines = [headers.map(escapeCsvField).join(',')];
+  for (const row of rows) {
+    const values = headers.map((h) => escapeCsvField(String(row[h] ?? '')));
+    lines.push(values.join(','));
+  }
+  return lines.join('\n');
+}
+
+export function createExportRoutes(db: DB): Router {
+  const router = Router();
+
+  // GET /events - Export events
+  router.get('/events', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const format = (req.query.format as string) || 'json';
+    const eventName = req.query.event_name as string | undefined;
+    const startDate = req.query.start_date as string | undefined;
+    const endDate = req.query.end_date as string | undefined;
+
+    let query = 'SELECT * FROM events WHERE tenant_id = ?';
+    const params: string[] = [user.tenant_id];
+
+    if (eventName) {
+      query += ' AND name = ?';
+      params.push(eventName);
+    }
+
+    if (startDate) {
+      query += ' AND timestamp >= ?';
+      params.push(startDate);
+    }
+
+    if (endDate) {
+      query += ' AND timestamp <= ?';
+      params.push(endDate);
+    }
+
+    query += ' ORDER BY timestamp DESC';
+
+    const events = db.prepare(query).all(...params) as EventRow[];
+
+    if (format === 'csv') {
+      const csvRows = events.map((e) => ({
+        id: e.id,
+        tenant_id: e.tenant_id,
+        name: e.name,
+        properties: e.properties,
+        timestamp: e.timestamp,
+        user_id: e.user_id || '',
+        session_id: e.session_id || '',
+      }));
+      const csv = toCsv(csvRows);
+      res.setHeader('Content-Type', 'text/csv');
+      res.setHeader('Content-Disposition', 'attachment; filename="events.csv"');
+      res.send(csv);
+    } else {
+      const parsed = events.map((e) => ({
+        ...e,
+        properties: JSON.parse(e.properties),
+      }));
+      res.json({ events: parsed });
+    }
+  });
+
+  // GET /dashboards/:id - Export dashboard data
+  router.get('/dashboards/:id', (req: Request, res: Response) => {
+    const user = getAuthUser(req);
+    if (!user) {
+      res.status(401).json({ error: 'Not authenticated' });
+      return;
+    }
+
+    const format = (req.query.format as string) || 'json';
+
+    const dashboard = db
+      .prepare('SELECT * FROM dashboards WHERE id = ? AND tenant_id = ?')
+      .get(req.params.id, user.tenant_id) as DashboardRow | undefined;
+
+    if (!dashboard) {
+      res.status(404).json({ error: 'Dashboard not found' });
+      return;
+    }
+
+    const widgets = db
+      .prepare('SELECT * FROM widgets WHERE dashboard_id = ?')
+      .all(dashboard.id) as WidgetRow[];
+
+    if (format === 'csv') {
+      const csvRows = [
+        {
+          id: dashboard.id,
+          name: dashboard.name,
+          owner_id: dashboard.owner_id,
+          config: dashboard.config,
+          created_at: dashboard.created_at,
+          updated_at: dashboard.updated_at,
+          widget_count: String(widgets.length),
+        },
+      ];
+      const csv = toCsv(csvRows);
+      res.setHeader('Content-Type', 'text/csv');
+      res.setHeader(
+        'Content-Disposition',
+        `attachment; filename="dashboard-${dashboard.id}.csv"`
+      );
+      res.send(csv);
+    } else {
+      res.json({
+        id: dashboard.id,
+        name: dashboard.name,
+        tenant_id: dashboard.tenant_id,
+        owner_id: dashboard.owner_id,
+        config: JSON.parse(dashboard.config),
+        created_at: dashboard.created_at,
+        updated_at: dashboard.updated_at,
+        widgets: widgets.map((w) => ({ ...w, config: JSON.parse(w.config) })),
+      });
+    }
+  });
+
+  return router;
+}
